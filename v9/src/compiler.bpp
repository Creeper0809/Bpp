    var tpl: *GenericFuncTemplate = new GenericFuncTemplate();
    tpl.name_ptr = name_ptr;
    tpl.name_len = name_len;
    tpl.mangled_ptr = 0;
    tpl.mangled_len = 0;
    tpl.params_vec = params_vec;
    tpl.func_sig = 0;
    tpl.func_def = 0;
    g_generic_funcs.put(name_ptr, name_len, tpl);
    return tpl;

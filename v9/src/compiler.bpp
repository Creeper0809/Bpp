func compiler_register_impl_method_ex(name_ptr: u64, name_len: u64, owner_struct_ptr: u64, owner_struct_len: u64, access: u64) -> u64 {
    if (name_ptr == 0 || name_len == 0) { return 0; }
    if (g_impl_methods == 0) { g_impl_methods = new HashMap<u64, u64>(64); }
    if (g_impl_method_access == 0) { g_impl_method_access = new HashMap<u64, u64>(64); }
    if (g_impl_method_owner == 0) { g_impl_method_owner = new HashMap<u64, *NameInfo>(64); }
    g_impl_methods.put(name_ptr, name_len, 1);
    g_impl_method_access.put(name_ptr, name_len, access);
    if (owner_struct_ptr != 0 && owner_struct_len != 0) {
        g_impl_method_owner.put(name_ptr, name_len, new NameInfo{owner_struct_ptr, owner_struct_len});
    }
    return 0;
}


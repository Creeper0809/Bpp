func compiler_global_exists(name_ptr: u64, name_len: u64) -> u64 {
    compiler_ensure_global_lookup();
    if (g_all_globals_map2 != 0 && g_all_globals_map2.has(name_ptr, name_len) != 0) {
        return true;
    }
    var resolved: *NameInfo = resolve_name(name_ptr, name_len);
    if (resolved != 0 && g_all_globals_map2 != 0) {
        if (g_all_globals_map2.has(resolved.ptr, resolved.len) != 0) { return true; }
    }
    return false;
}

func compiler_find_global_info(name_ptr: u64, name_len: u64) -> *GlobalInfo {
    compiler_ensure_global_lookup();
    var ginfo: *GlobalInfo = g_all_globals_map2.?get(name_ptr, name_len);
    if (ginfo != 0) { return ginfo; }
    var resolved: *NameInfo = resolve_name(name_ptr, name_len);
    if (resolved != 0) {
        return g_all_globals_map2.?get(resolved.ptr, resolved.len);
    }
    return 0;
}

func compiler_set_all_globals(globals: *Vec<*GlobalInfo>) -> u64 {

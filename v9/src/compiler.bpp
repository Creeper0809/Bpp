func compiler_rebuild_func_lookup() -> u64 {
    if (g_all_funcs == 0) {
        g_all_funcs_map = new HashMap<u64, *AstFunc>(16);
        g_all_funcs_map_src = 0;
        g_all_funcs_map_src_len = 0;
        return 0;
    }
    var n: u64 = g_all_funcs.len();
    var cap: u64 = 16;
    if (n > 0) {
        cap = n * 2;
        if (cap < 16) { cap = 16; }
    }
    g_all_funcs_map = new HashMap<u64, *AstFunc>(cap);
    for (var i: u64 = 0; i < n; i++) {
        var fn: *AstFunc = g_all_funcs.get(i);
        if (fn == 0) { continue; }
        if (g_all_funcs_map.has(fn.name_ptr, fn.name_len) != 0) { continue; }
        g_all_funcs_map.put(fn.name_ptr, fn.name_len, fn);
    }
    g_all_funcs_map_src = g_all_funcs;
    g_all_funcs_map_src_len = n;
    return 0;
}

func compiler_extend_func_lookup(from_idx: u64, to_idx: u64) -> u64 {
    if (g_all_funcs_map == 0 || g_all_funcs == 0) { return 0; }
    for (var i: u64 = from_idx; i < to_idx; i++) {
        var fn: *AstFunc = g_all_funcs.get(i);
        if (fn == 0) { continue; }
        if (g_all_funcs_map.has(fn.name_ptr, fn.name_len) != 0) { continue; }
        g_all_funcs_map.put(fn.name_ptr, fn.name_len, fn);
    }
    return 0;
}

func compiler_ensure_func_lookup() -> u64 {
    if (g_all_funcs == 0) {
        if (g_all_funcs_map == 0 || g_all_funcs_map_src != 0 || g_all_funcs_map_src_len != 0) {
            compiler_rebuild_func_lookup();
        }
        return 0;
    }
    var n: u64 = g_all_funcs.len();
    if (g_all_funcs_map == 0 || g_all_funcs_map_src != g_all_funcs || g_all_funcs_map_src_len > n) {
        compiler_rebuild_func_lookup();
        return 0;
    }
    if (g_all_funcs_map_src_len < n) {
        compiler_extend_func_lookup(g_all_funcs_map_src_len, n);
        g_all_funcs_map_src_len = n;
    }
    return 0;
}


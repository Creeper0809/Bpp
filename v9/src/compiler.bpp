        g_abst_methods_access = new HashMap<u64, *HashMap<u64, u64>>(32);
    }
    var m: *HashMap<u64, u64> = g_abst_methods_access.get(struct_ptr, struct_len);
    if (m != 0) { return m; }
    var out: *HashMap<u64, u64> = new HashMap<u64, u64>(16);
    g_abst_methods_access.put(struct_ptr, struct_len, out);
    return out;
}


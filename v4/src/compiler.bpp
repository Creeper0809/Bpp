func generic_struct_get_or_create_instance(tpl_ptr: u64, bindings: u64) -> u64 {
    var tpl: *GenericStructTemplate = (*GenericStructTemplate)tpl_ptr;
    var name_info: *NameInfo = (*NameInfo)generic_mangle_name(tpl->name_ptr, tpl->name_len, tpl->params_vec, bindings);
    var inst_ptr: u64 = name_info->ptr;
    var inst_len: u64 = name_info->len;

    if (g_generic_struct_instantiations != 0) {
        var exists: *AstStructDef = ((*HashMap<u64, *AstStructDef>)g_generic_struct_instantiations)->get(inst_ptr, inst_len);
        if (exists != 0) {
            compiler_register_generic_struct_bindings(inst_ptr, inst_len, bindings);
            return (u64)name_info;
        }
    }

    var def_src: u64 = tpl->struct_def;
    if (def_src == 0) {
        emit_stderr("[ERROR] Missing generic struct template\n", 46);
        panic("Parse error");
    }

    var struct_new: u64 = generic_clone_struct(def_src, bindings, inst_ptr, inst_len);
    register_struct_type(struct_new);

    if (g_generic_struct_instantiations == 0) { g_generic_struct_instantiations = HashMap.new<u64, *AstStructDef>(64); }
    ((*HashMap<u64, *AstStructDef>)g_generic_struct_instantiations)->put(inst_ptr, inst_len, (*AstStructDef)struct_new);
    compiler_register_generic_struct_bindings(inst_ptr, inst_len, bindings);
    return (u64)name_info;
}

func generic_build_bindings_by_infer(tpl_ptr: u64, call_args: u64, symtab: u64) -> u64 {
    var tpl: *GenericFuncTemplate = (*GenericFuncTemplate)tpl_ptr;
    var bindings: *Vec<*GenericBinding> = Vec.new<*GenericBinding>(4);
    var def_src: u64 = tpl->func_def;
    if (def_src == 0) { def_src = tpl->func_sig; }
    if (def_src == 0) {
        emit_stderr("[ERROR] Missing generic signature for inference\n", 51);
        panic("Parse error");
    }
    var fn: *AstFunc = (*AstFunc)def_src;
    var params: u64 = fn->params_vec;
    var pn: u64 = ((*Vec<u64>)params)->len();
    var an: u64 = ((*Vec<u64>)call_args)->len();
    if (pn != an) {
        emit_stderr("[ERROR] Generic inference arg count mismatch\n", 49);
        panic("Parse error");

    for (var i: u64 = 0; i < pn; i++) {
        var p: *Param = (*Param)((*Vec<u64>)params)->get(i);
        var arg_expr: u64 = ((*Vec<u64>)call_args)->get(i);
        var arg_ti: u64 = generic_infer_expr_type(arg_expr, symtab);
        if (generic_infer_from_typeinfo(p->type_kind, p->ptr_depth, p->struct_name_ptr, p->struct_name_len, p->elem_type_kind, p->elem_ptr_depth, p->array_len, p->array_len_is_param, p->array_len_param_ptr, p->array_len_param_len, arg_ti, (u64)bindings) != 0) {
            return 0;
        }
    }

    return (u64)bindings;
}

func generic_resolve_template(name_ptr: u64, name_len: u64) -> u64 {
    var tpl: u64 = compiler_find_generic_template_by_name(name_ptr, name_len);
    if (tpl != 0) {
        var t: *GenericFuncTemplate = (*GenericFuncTemplate)tpl;
        if (t->params_vec != 0 && ((*Vec<u64>)t->params_vec)->len() > 0) { return tpl; }
    }
    var resolved: u64 = resolve_name(name_ptr, name_len);
    if (resolved != 0) {
        var info: *NameInfo = (*NameInfo)resolved;
        tpl = compiler_find_generic_template_by_mangled(info->ptr, info->len);
        if (tpl != 0) {
            var t2: *GenericFuncTemplate = (*GenericFuncTemplate)tpl;
            if (t2->params_vec != 0 && ((*Vec<u64>)t2->params_vec)->len() > 0) { return tpl; }
            tpl = 0;
        }
    }
    return tpl;
}

func generic_process_expr(expr: u64, symtab: u64) -> u64 {
    if (expr == 0) { return 0; }
    var kind: u64 = ast_kind(expr);
    switch (kind) {
        case AST_GENERIC_CALL:
            var gc: *AstGenericCall = (*AstGenericCall)expr;
            var tpl_ptr: u64 = generic_resolve_template(gc->name_ptr, gc->name_len);
            if (tpl_ptr == 0) {
                emit_stderr("[ERROR] Generic function not found\n", 36);
                panic("Parse error");
            }
            var bindings: u64 = generic_build_bindings_for_explicit(tpl_ptr, gc->generic_args_vec);
            var name_info: *NameInfo = (*NameInfo)generic_get_or_create_instance(tpl_ptr, bindings);
            var args_new: *Vec<u64> = Vec.new<u64>(4);
            var an: u64 = ((*Vec<u64>)gc->args_vec)->len();
            for (var i: u64 = 0; i < an; i++) {
                ((*Vec<u64>)args_new)->push(generic_process_expr(((*Vec<u64>)gc->args_vec)->get(i), symtab));
            }
            return ast_call(name_info->ptr, name_info->len, (u64)args_new);
        case AST_CALL:
            var call: *AstCall = (*AstCall)expr;
            var tpl_ptr2: u64 = generic_resolve_template(call->name_ptr, call->name_len);
            if (tpl_ptr2 == 0) {
                var args_new2: *Vec<u64> = Vec.new<u64>(4);
                var an2: u64 = ((*Vec<u64>)call->args_vec)->len();
                for (var i2: u64 = 0; i2 < an2; i2++) {
                    ((*Vec<u64>)args_new2)->push(generic_process_expr(((*Vec<u64>)call->args_vec)->get(i2), symtab));
                }
                return ast_call(call->name_ptr, call->name_len, (u64)args_new2);
            }
            var bindings2: u64 = generic_build_bindings_by_infer(tpl_ptr2, call->args_vec, symtab);
            if (bindings2 == 0) {
                var args_fallback: *Vec<u64> = Vec.new<u64>(4);
                var an_fallback: u64 = ((*Vec<u64>)call->args_vec)->len();
                for (var ifb: u64 = 0; ifb < an_fallback; ifb++) {
                    ((*Vec<u64>)args_fallback)->push(generic_process_expr(((*Vec<u64>)call->args_vec)->get(ifb), symtab));
                }
                return ast_call(call->name_ptr, call->name_len, (u64)args_fallback);
            }
            var name_info2: *NameInfo = (*NameInfo)generic_get_or_create_instance(tpl_ptr2, bindings2);
            var args_new3: *Vec<u64> = Vec.new<u64>(4);
            var an3: u64 = ((*Vec<u64>)call->args_vec)->len();
            for (var i3: u64 = 0; i3 < an3; i3++) {
                ((*Vec<u64>)args_new3)->push(generic_process_expr(((*Vec<u64>)call->args_vec)->get(i3), symtab));
            }
            return ast_call(name_info2->ptr, name_info2->len, (u64)args_new3);
        case AST_BINARY:
            var bin: *AstBinary = (*AstBinary)expr;
            bin->left = generic_process_expr(bin->left, symtab);
            bin->right = generic_process_expr(bin->right, symtab);
            return expr;
        case AST_UNARY:
            var un: *AstUnary = (*AstUnary)expr;
            un->operand = generic_process_expr(un->operand, symtab);
            return expr;
        case AST_ADDR_OF:
            var ao: *AstAddrOf = (*AstAddrOf)expr;
            ao->operand = generic_process_expr(ao->operand, symtab);
            return expr;
        case AST_DEREF:
            var dr: *AstDeref = (*AstDeref)expr;
            dr->operand = generic_process_expr(dr->operand, symtab);
            return expr;
        case AST_DEREF8:
            var dr8: *AstDeref8 = (*AstDeref8)expr;
            dr8->operand = generic_process_expr(dr8->operand, symtab);
            return expr;
        case AST_CAST:
            var cs: *AstCast = (*AstCast)expr;
            cs->expr = generic_process_expr(cs->expr, symtab);
            return expr;
        case AST_MEMBER_ACCESS:
            var ma: *AstMemberAccess = (*AstMemberAccess)expr;
            ma->object = generic_process_expr(ma->object, symtab);
            return expr;
        case AST_METHOD_CALL:
            var mc: *AstMethodCall = (*AstMethodCall)expr;
            mc->receiver = generic_process_expr(mc->receiver, symtab);
            var args: *Vec<u64> = Vec.new<u64>(4);
            var an4: u64 = ((*Vec<u64>)mc->args_vec)->len();
            for (var i4: u64 = 0; i4 < an4; i4++) {
                ((*Vec<u64>)args)->push(generic_process_expr(((*Vec<u64>)mc->args_vec)->get(i4), symtab));
            }
            mc->args_vec = (u64)args;
            var recv_ti_ptr: u64 = generic_infer_expr_type(mc->receiver, symtab);
            if (recv_ti_ptr != 0) {
                var recv_ti: *TypeInfo = (*TypeInfo)recv_ti_ptr;
                if (recv_ti->type_kind == TYPE_STRUCT) {
                    var struct_ptr: u64 = recv_ti->struct_name_ptr;
                    var struct_len: u64 = recv_ti->struct_name_len;
                    var base_len: u64 = compiler_find_generic_suffix_index(struct_ptr, struct_len);
                    var full_len2: u64 = base_len + 1 + mc->method_len;
                    var full_ptr2: u64 = heap_alloc((full_len2 + 1) * sizeof(u8));
                    for (var bi: u64 = 0; bi < base_len; bi++) {
                        *(*u8)(full_ptr2 + bi) = *(*u8)(struct_ptr + bi);
                    }
                    *(*u8)(full_ptr2 + base_len) = 95;
                    for (var mj: u64 = 0; mj < mc->method_len; mj++) {
                        *(*u8)(full_ptr2 + base_len + 1 + mj) = *(*u8)(mc->method_ptr + mj);
                    }
                    *(*u8)(full_ptr2 + full_len2) = 0;

                    var tpl_ptr3: u64 = compiler_find_generic_template_by_name(full_ptr2, full_len2);
                    if (tpl_ptr3 != 0) {
                        var bindings3: u64 = 0;
                        if (base_len != struct_len) {
                            bindings3 = (u64)compiler_get_generic_struct_bindings(struct_ptr, struct_len);
                        }
                        if (bindings3 == 0) {
                            var infer_args: *Vec<u64> = Vec.new<u64>(an4 + 1);
                            ((*Vec<u64>)infer_args)->push(ast_addr_of(mc->receiver));
                            for (var ai: u64 = 0; ai < an4; ai++) {
                                ((*Vec<u64>)infer_args)->push(((*Vec<u64>)args)->get(ai));
                            }
                            bindings3 = generic_build_bindings_by_infer(tpl_ptr3, (u64)infer_args, symtab);
                        }
                        if (bindings3 != 0) {
                            generic_get_or_create_instance(tpl_ptr3, bindings3);
                        }
                    }
                }
            }
            return expr;
        case AST_INDEX:
            var ix: *AstIndex = (*AstIndex)expr;
            ix->base = generic_process_expr(ix->base, symtab);
            ix->index = generic_process_expr(ix->index, symtab);
            return expr;
        case AST_SLICE:
            var sl: *AstSlice = (*AstSlice)expr;
            sl->ptr_expr = generic_process_expr(sl->ptr_expr, symtab);
            sl->len_expr = generic_process_expr(sl->len_expr, symtab);
            return expr;
        case AST_CALL_PTR:
            var cp: *AstCallPtr = (*AstCallPtr)expr;
            cp->callee = generic_process_expr(cp->callee, symtab);
            var args5: *Vec<u64> = Vec.new<u64>(4);
            var an5: u64 = ((*Vec<u64>)cp->args_vec)->len();
            for (var i5: u64 = 0; i5 < an5; i5++) {
                ((*Vec<u64>)args5)->push(generic_process_expr(((*Vec<u64>)cp->args_vec)->get(i5), symtab));
            }
            cp->args_vec = (u64)args5;
            return expr;
        case AST_SIZEOF_EXPR:
            var se: *AstSizeofExpr = (*AstSizeofExpr)expr;
            se->expr = generic_process_expr(se->expr, symtab);
            return expr;
        default:
            return expr;
    }
}

func generic_process_stmt(stmt: u64, symtab: u64) -> u64 {
    if (stmt == 0) { return 0; }
    var kind: u64 = ast_kind(stmt);
    switch (kind) {
        case AST_RETURN:
            var r: *AstReturn = (*AstReturn)stmt;
            r->expr = generic_process_expr(r->expr, symtab);
            return stmt;
        case AST_VAR_DECL:
            var vd: *AstVarDecl = (*AstVarDecl)stmt;
            vd->init_expr = generic_process_expr(vd->init_expr, symtab);
            return stmt;
        case AST_ASSIGN:
            var assign_stmt: *AstAssign = (*AstAssign)stmt;
            assign_stmt->target = generic_process_expr(assign_stmt->target, symtab);
            assign_stmt->value = generic_process_expr(assign_stmt->value, symtab);
            return stmt;
        case AST_EXPR_STMT:
            var es: *AstExprStmt = (*AstExprStmt)stmt;
            es->expr = generic_process_expr(es->expr, symtab);
            return stmt;
        case AST_IF:
            var ifs: *AstIf = (*AstIf)stmt;
            ifs->cond = generic_process_expr(ifs->cond, symtab);
            generic_process_stmt(ifs->then_block, symtab);
            generic_process_stmt(ifs->else_block, symtab);
            return stmt;
        case AST_WHILE:
            var wl: *AstWhile = (*AstWhile)stmt;
            wl->cond = generic_process_expr(wl->cond, symtab);
            generic_process_stmt(wl->body, symtab);
            return stmt;
        case AST_FOR:
            var fr: *AstFor = (*AstFor)stmt;
            generic_process_stmt(fr->init, symtab);
            fr->cond = generic_process_expr(fr->cond, symtab);
            fr->update = generic_process_expr(fr->update, symtab);
            generic_process_stmt(fr->body, symtab);
            return stmt;
        case AST_SWITCH:
            var sw: *AstSwitch = (*AstSwitch)stmt;
            sw->expr = generic_process_expr(sw->expr, symtab);
            var cases: u64 = sw->cases_vec;
            var cn: u64 = ((*Vec<u64>)cases)->len();
            for (var i: u64 = 0; i < cn; i++) {
                generic_process_stmt(((*Vec<u64>)cases)->get(i), symtab);
            }
            return stmt;
        case AST_CASE:
            var cs: *AstCase = (*AstCase)stmt;
            generic_process_stmt(cs->body, symtab);
            return stmt;
        case AST_BLOCK:
            var blk: *AstBlock = (*AstBlock)stmt;
            var stmts: u64 = blk->stmts_vec;
            var n: u64 = ((*Vec<u64>)stmts)->len();
            for (var i2: u64 = 0; i2 < n; i2++) {
                generic_process_stmt(((*Vec<u64>)stmts)->get(i2), symtab);
            }
            return stmt;
        default:
            return stmt;
    }
}

func compiler_is_generic_template(fn_ptr: u64) -> u64 {
    if (compiler_is_generic_func_ptr(fn_ptr) != 0) { return 1; }
    var fn: *AstFunc = (*AstFunc)fn_ptr;
    if (fn->ret_type == TYPE_GENERIC) { return 1; }

    var params: u64 = fn->params_vec;
    if (params == 0) { return 0; }

    var n: u64 = ((*Vec<u64>)params)->len();
    for (var i: u64 = 0; i < n; i++) {
        var p: *Param = (*Param)((*Vec<u64>)params)->get(i);
        if (p->type_kind == TYPE_GENERIC) { return 1; }
        if (p->elem_type_kind == TYPE_GENERIC) { return 1; }
        if (p->array_len_is_param != 0) { return 1; }
    }
    return 0;
}

func compiler_monomorphize_generics() -> u64 {
    if (g_generic_func_ptrs == 0 || ((*Vec<*GenericFuncTemplate>)g_generic_func_ptrs)->len() == 0) {
        return 0;
    }
    if (g_all_funcs == 0) { return 0; }
    for (var i2: u64 = 0; i2 < ((*Vec<*AstFunc>)g_all_funcs)->len(); i2++) {
        var fn_ptr: u64 = (u64)((*Vec<*AstFunc>)g_all_funcs)->get(i2);
        if (compiler_is_generic_template(fn_ptr) != 0) { continue; }
        var fn: *AstFunc = (*AstFunc)fn_ptr;
        var symtab: u64 = generic_build_symtab_for_func(fn_ptr);
        generic_process_stmt(fn->body, symtab);
    }

    var filtered_funcs: *Vec<*AstFunc> = Vec.new<*AstFunc>(((*Vec<*AstFunc>)g_all_funcs)->len());
    var total_funcs: u64 = ((*Vec<*AstFunc>)g_all_funcs)->len();
    for (var fi: u64 = 0; fi < total_funcs; fi++) {
        var fn_ptr2: u64 = (u64)((*Vec<*AstFunc>)g_all_funcs)->get(fi);
        if (compiler_is_generic_template(fn_ptr2) != 0) { continue; }
        ((*Vec<*AstFunc>)filtered_funcs)->push((*AstFunc)fn_ptr2);
    }
    g_all_funcs = filtered_funcs;
    return 0;
}


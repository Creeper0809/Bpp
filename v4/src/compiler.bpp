// Pass1: scan signatures and register generic templates before full parse.
func load_module_pass1(file_path: u64, file_path_len: u64) -> u64 {
    if (g_loaded_modules_pass1 == 0) { g_loaded_modules_pass1 = HashMap.new<u64, u64>(64); }
    if (((*HashMap<u64, u64>)g_loaded_modules_pass1)->has(file_path, file_path_len)) {
        return 1;
    }
    ((*HashMap<u64, u64>)g_loaded_modules_pass1)->put(file_path, file_path_len, 1);

    var content: u64 = read_entire_file(file_path);
    if (content == 0) {
        emit_stderr("[ERROR] Cannot open module: ", 29);
        for (var i: u64 = 0; i < file_path_len; i++) {
            emit_char(*(*u8)(file_path + i));
        }
        emit_nl();
        return 0;
    }

    var src: u64 = g_file_ptr;
    var slen: u64 = g_file_len;
    var tokens: u64 = lex_all(src, slen);

    // Pre-scan imports to ensure templates are registered before signature parse.
    var p0: u64 = parse_new(tokens);
    var imports_only: *Vec<*AstImport> = (*Vec<*AstImport>)parse_program_imports_only(p0);
    var imports_copy: *Vec<*AstImport> = Vec.new<*AstImport>(4);
    var num_imports: u64 = ((*Vec<*AstImport>)imports_only)->len();
    for (var ii: u64 = 0; ii < num_imports; ii++) {
        var imp: *AstImport = ((*Vec<*AstImport>)imports_only)->get(ii);
        var imp_path: u64 = imp->path_ptr;
        var imp_len: u64 = imp->path_len;
        var path_copy: u64 = heap_alloc((imp_len + 1) * sizeof(u8));
        for (var ci: u64 = 0; ci < imp_len; ci++) {
            *(*u8)(path_copy + ci) = *(*u8)(imp_path + ci);
        }
        *(*u8)(path_copy + imp_len) = 0;
        var imp_copy: *AstImport = (*AstImport)ast_import(path_copy, imp_len, 0, 0, 0, 0);
        ((*Vec<*AstImport>)imports_copy)->push(imp_copy);
    }
    if (g_module_imports_pass1 == 0) { g_module_imports_pass1 = HashMap.new<u64, *Vec<*AstImport> >(64); }
    ((*HashMap<u64, *Vec<*AstImport> >)g_module_imports_pass1)->put(file_path, file_path_len, imports_copy);

    for (var ii2: u64 = 0; ii2 < num_imports; ii2++) {
        var imp2: *AstImport = ((*Vec<*AstImport>)imports_copy)->get(ii2);
        var imp_path2: u64 = imp2->path_ptr;
        var imp_len2: u64 = imp2->path_len;
        var resolved: u64 = resolve_module_path(imp_path2, imp_len2);
        var resolved_len: u64 = str_len(resolved);
        if (!load_module_pass1(resolved, resolved_len)) {
            return 0;
        }
    }

    var p1: u64 = parse_new(tokens);
    var prog_sig: u64 = parse_program_pass1(p1);

    var prog_sig_info: *AstProgram = (*AstProgram)prog_sig;
    var sig_funcs: *Vec<*AstFunc> = (*Vec<*AstFunc>)prog_sig_info->funcs_vec;
    var num_sig_funcs: u64 = ((*Vec<*AstFunc>)sig_funcs)->len();
    for (var sfi: u64 = 0; sfi < num_sig_funcs; sfi++) {
        ((*Vec<*AstFunc>)g_all_func_sigs)->push(((*Vec<*AstFunc>)sig_funcs)->get(sfi));
    }
    return 1;
}


func load_module(file_path: u64, file_path_len: u64) -> u64 {
    if (((*HashMap<u64, u64>)g_loaded_modules)->has(file_path, file_path_len)) {
        return 1;
    }

    if (!load_module_pass1(file_path, file_path_len)) {
        return 0;
    }

    ((*HashMap<u64, u64>)g_loaded_modules)->put(file_path, file_path_len, 1);

    var path_copy2: u64 = heap_alloc((file_path_len + 1) * sizeof(u8));
    for (var pc: u64 = 0; pc < file_path_len; pc++) {
        *(*u8)(path_copy2 + pc) = *(*u8)(file_path + pc);
    }
    *(*u8)(path_copy2 + file_path_len) = 0;
    var path_info: *NameInfo = (*NameInfo)heap_alloc(sizeof(NameInfoLocal));
    path_info->ptr = path_copy2;
    path_info->len = file_path_len;
    ((*Vec<*NameInfo>)g_loaded_modules_vec)->push(path_info);

    // Ensure imports are fully parsed before this module (type resolution).
    if (g_module_imports_pass1 != 0) {
        var imports_p1: *Vec<*AstImport> = ((*HashMap<u64, *Vec<*AstImport> >)g_module_imports_pass1)->get(file_path, file_path_len);
        if (imports_p1 != 0) {
            var num_imports_p1: u64 = ((*Vec<*AstImport>)imports_p1)->len();
            for (var pi: u64 = 0; pi < num_imports_p1; pi++) {
                var imp_p1: *AstImport = ((*Vec<*AstImport>)imports_p1)->get(pi);
                var imp_path_p1: u64 = imp_p1->path_ptr;
                var imp_len_p1: u64 = imp_p1->path_len;
                var resolved_p1: u64 = resolve_module_path(imp_path_p1, imp_len_p1);
                var resolved_len_p1: u64 = str_len(resolved_p1);
                if (!load_module(resolved_p1, resolved_len_p1)) {
                    return 0;
                }
            }
        }
    }

    var content: u64 = read_entire_file(file_path);
    if (content == 0) {
        emit_stderr("[ERROR] Cannot open module: ", 29);
        for (var i: u64 = 0; i< file_path_len;i++){
            emit_char(*(*u8)(file_path + i));
        }
        emit_nl();
        return 0;
    }

    var src: u64 = g_file_ptr;
    var slen: u64  = g_file_len;

    var tokens: u64 = lex_all(src, slen);
    
    // Module identity for mangling
    var module_id: u64 = module_id_from_path(file_path, file_path_len);
    var module_info: *NameInfo = (*NameInfo)module_id;
    var module_ptr: u64 = module_info->ptr;
    var module_len: u64 = module_info->len;
    var module_prefix: u64 = module_prefix_from_id(module_ptr, module_len);
    var prefix_info: *NameInfo = (*NameInfo)module_prefix;
    var prefix_ptr: u64 = prefix_info->ptr;
    var prefix_len: u64 = prefix_info->len;
    
    // Pass 1: collect function signatures only
    var p1: u64 = parse_new(tokens);
    var prog_sig: u64 = parse_program_pass1(p1);

    var prog_sig_info: *AstProgram = (*AstProgram)prog_sig;
    var sig_funcs: *Vec<*AstFunc> = (*Vec<*AstFunc>)prog_sig_info->funcs_vec;
    var num_sig_funcs: u64 = ((*Vec<*AstFunc>)sig_funcs)->len();
    for (var sfi: u64 = 0; sfi < num_sig_funcs; sfi++) {
        ((*Vec<*AstFunc>)g_all_func_sigs)->push(((*Vec<*AstFunc>)sig_funcs)->get(sfi));
    }

    // Pass 2: full parse with bodies
    var p: u64 = parse_new(tokens);
    var prog: u64 = parse_program(p);

    // Register exports + mangle symbols
    register_module_exports(prog, module_ptr, module_len, prefix_ptr, prefix_len);
    compiler_generic_update_mangled_names(prog);

    // Store imports for deferred alias resolution
    var prog_info2: *AstProgram = (*AstProgram)prog;
    var imports: *Vec<*AstImport> = (*Vec<*AstImport>)prog_info2->imports_vec;
    var imports_copy: *Vec<*AstImport> = Vec.new<*AstImport>(4);
    var num_imports: u64 = ((*Vec<*AstImport>)imports)->len();
    for (var ii: u64 = 0; ii < num_imports; ii++) {
        var imp: *AstImport = ((*Vec<*AstImport>)imports)->get(ii);
        var imp_path: u64 = imp->path_ptr;
        var imp_len: u64 = imp->path_len;
        var imp_sym_ptr: u64 = imp->symbol_ptr;
        var imp_sym_len: u64 = imp->symbol_len;
        var imp_alias_ptr: u64 = imp->alias_ptr;
        var imp_alias_len: u64 = imp->alias_len;

        var path_copy: u64 = heap_alloc((imp_len + 1) * sizeof(u8));
        for (var ci: u64 = 0; ci < imp_len; ci++) {
            *(*u8)(path_copy + ci) = *(*u8)(imp_path + ci);
        }
        *(*u8)(path_copy + imp_len) = 0;

        var sym_copy: u64 = 0;
        var alias_copy: u64 = 0;
        if (imp_sym_ptr != 0) {
            sym_copy = heap_alloc((imp_sym_len + 1) * sizeof(u8));
            for (var si: u64 = 0; si < imp_sym_len; si++) {
                *(*u8)(sym_copy + si) = *(*u8)(imp_sym_ptr + si);
            }
            *(*u8)(sym_copy + imp_sym_len) = 0;

            alias_copy = heap_alloc((imp_alias_len + 1) * sizeof(u8));
            for (var ai: u64 = 0; ai < imp_alias_len; ai++) {
                *(*u8)(alias_copy + ai) = *(*u8)(imp_alias_ptr + ai);
            }
            *(*u8)(alias_copy + imp_alias_len) = 0;
        }

        var imp_copy: *AstImport = (*AstImport)ast_import(path_copy, imp_len, sym_copy, imp_sym_len, alias_copy, imp_alias_len);
        ((*Vec<*AstImport>)imports_copy)->push(imp_copy);
    }
    ((*HashMap<u64, *Vec<*AstImport> >)g_module_imports_pass2)->put(file_path, file_path_len, imports_copy);

    // Add consts
    var consts: *Vec<*AstConstDecl> = (*Vec<*AstConstDecl>)prog_info2->consts_vec;
    var num_consts: u64  = ((*Vec<*AstConstDecl>)consts)->len();
    for (var ci: u64 = 0; ci < num_consts; ci++){
        ((*Vec<*AstConstDecl>)g_all_consts)->push(((*Vec<*AstConstDecl>)consts)->get(ci));
    }

    // Add funcs
    var funcs: *Vec<*AstFunc> = (*Vec<*AstFunc>)prog_info2->funcs_vec;
    var num_funcs: u64 = ((*Vec<*AstFunc>)funcs)->len();
    for (var fi: u64 = 0; fi < num_funcs; fi++){
        var fn: *AstFunc = ((*Vec<*AstFunc>)funcs)->get(fi);
        ((*Vec<*AstFunc>)g_all_funcs)->push(fn);
    }

    // Add globals
    var globals: *Vec<*GlobalInfo>  = (*Vec<*GlobalInfo>)prog_info2->globals_vec;
    if (globals != 0) {
        var num_globals: u64  = ((*Vec<*GlobalInfo>)globals)->len();
        for (var gi: u64 = 0; gi < num_globals; gi++){
            ((*Vec<*GlobalInfo>)g_all_globals)->push(((*Vec<*GlobalInfo>)globals)->get(gi));
        }
    }

    // Register structs
    var structs: *Vec<*AstStructDef> = (*Vec<*AstStructDef>)prog_info2->structs_vec;
    if (structs != 0) {
        var num_structs: u64 = ((*Vec<*AstStructDef>)structs)->len();
        for (var si: u64 = 0; si < num_structs; si++){
            var struct_info: *AstStructDef = ((*Vec<*AstStructDef>)structs)->get(si);
            var struct_name_ptr: u64 = struct_info->name_ptr;
            var struct_name_len: u64 = struct_info->name_len;
            ((*HashMap<u64, *AstStructDef>)g_all_structs)->put(struct_name_ptr, struct_name_len, struct_info);
        }
    }

    return 1;
}


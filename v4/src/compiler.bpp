func resolve_name(name_ptr: u64, name_len: u64) -> u64 {
    var suffix_idx: u64 = compiler_find_generic_suffix_index(name_ptr, name_len);
    if (suffix_idx < name_len) {
        var base_ptr: u64 = name_ptr;
        var base_len: u64 = suffix_idx;
        var resolved_base: u64 = resolve_name(base_ptr, base_len);
        if (resolved_base != 0) {
            var base_info: *NameInfo = (*NameInfo)resolved_base;
            var suffix_len: u64 = name_len - suffix_idx;
            var combined_len: u64 = base_info->len + suffix_len;
            var combined_ptr: u64 = heap_alloc((combined_len + 1) * sizeof(u8));
            for (var i: u64 = 0; i < base_info->len; i++) {
                *(*u8)(combined_ptr + i) = *(*u8)(base_info->ptr + i);
            }
            for (var j: u64 = 0; j < suffix_len; j++) {
                *(*u8)(combined_ptr + base_info->len + j) = *(*u8)(name_ptr + suffix_idx + j);
            }
            *(*u8)(combined_ptr + combined_len) = 0;
            var info_out: *NameInfo = (*NameInfo)heap_alloc(sizeof(NameInfoLocal));
            info_out->ptr = combined_ptr;
            info_out->len = combined_len;
            return (u64)info_out;
        }
    }
    if (g_current_module_ptr != 0 && g_current_module_len != 0) {
        var alias: *NameInfo = resolve_import_alias(g_current_module_ptr, g_current_module_len, name_ptr, name_len);
        if (alias != 0) { return (u64)alias; }
    }

    if (g_current_module_ptr != 0 && g_current_module_len != 0) {
        var exports: *Vec<*ExportEntry> = module_exports_get(g_current_module_ptr, g_current_module_len);
        if (exports != 0) {
            var n: u64 = ((*Vec<*ExportEntry>)exports)->len();
            for (var i: u64 = 0; i < n; i++) {
                var e: *ExportEntry = ((*Vec<*ExportEntry>)exports)->get(i);
                var n_ptr: u64 = e->name_ptr;
                var n_len: u64 = e->name_len;
                if (str_eq(n_ptr, n_len, name_ptr, name_len)) {
                    var info: *NameInfo = (*NameInfo)heap_alloc(sizeof(NameInfoLocal));
                    info->ptr = e->mangled_ptr;
                    info->len = e->mangled_len;
                    return (u64)info;
                }
            }
        }
    }

    return 0;
}


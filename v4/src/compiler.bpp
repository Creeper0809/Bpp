            return ast_block((u64)stmts_out);
        case AST_ASM:
            var asmnode: *AstAsm = (*AstAsm)node;
            var text_vec: *Vec<u64> = Vec.new<u64>(16);
            var tn: u64 = ((*Vec<u64>)asmnode->text_vec)->len();
            for (var i2: u64 = 0; i2 < tn; i2++) { ((*Vec<u64>)text_vec)->push(((*Vec<u64>)asmnode->text_vec)->get(i2)); }
            return ast_asm((u64)text_vec);
        default:
            return node;
    }
}


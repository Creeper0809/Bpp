func compiler_generic_update_mangled_names(prog: u64) -> u64 {
    if (g_generic_func_ptrs == 0) { return 0; }
    var prog_info: *AstProgram = (*AstProgram)prog;
    var funcs: *Vec<*AstFunc> = (*Vec<*AstFunc>)prog_info->funcs_vec;
    if (funcs == 0) { return 0; }
    var n: u64 = ((*Vec<*AstFunc>)funcs)->len();
    for (var i: u64 = 0; i < n; i++) {
        var fn: *AstFunc = ((*Vec<*AstFunc>)funcs)->get(i);
        var tpl_ptr: u64 = compiler_find_generic_template_by_func_ptr((u64)fn);
        if (tpl_ptr == 0) { continue; }
        var tpl: *GenericFuncTemplate = (*GenericFuncTemplate)tpl_ptr;
        tpl->mangled_ptr = fn->name_ptr;
        tpl->mangled_len = fn->name_len;
        if (g_generic_funcs_mangled == 0) { g_generic_funcs_mangled = HashMap.new<u64, *GenericFuncTemplate>(64); }
        ((*HashMap<u64, *GenericFuncTemplate>)g_generic_funcs_mangled)->put(tpl->mangled_ptr, tpl->mangled_len, tpl);
    }
    return 0;
}


// Append a string to a vec<u8>
func generic_vec_append_str(v: *Vec<u64>, s_ptr: u64, s_len: u64) -> u64 {
    for (var i: u64 = 0; i < s_len; i++) {
        ((*Vec<u64>)v)->push(*(*u8)(s_ptr + i));
    }
    return 0;
}

// Append unsigned integer to vec as decimal
func generic_vec_append_u64(v: *Vec<u64>, value: u64) -> u64 {
    if (value == 0) {
        ((*Vec<u64>)v)->push(48);
        return 0;
    }
    var buf: u64 = heap_alloc(32 * sizeof(u8));
    var idx: u64 = 0;
    var x: u64 = value;
    while (x > 0) {
        var digit: u64 = x % 10;
        *(*u8)(buf + idx) = 48 + digit;
        idx = idx + 1;
        x = x / 10;
    }
    for (var i: u64 = 0; i < idx; i++) {
        ((*Vec<u64>)v)->push(*(*u8)(buf + (idx - 1 - i)));
    }
    return 0;
}

func generic_type_equal(a: u64, b: u64) -> u64 {
    var ta: *TypeInfo = (*TypeInfo)a;
    var tb: *TypeInfo = (*TypeInfo)b;
    if (ta->type_kind != tb->type_kind) { return 0; }
    if (ta->ptr_depth != tb->ptr_depth) { return 0; }
    if (ta->type_kind == TYPE_STRUCT) {
        if (!str_eq(ta->struct_name_ptr, ta->struct_name_len, tb->struct_name_ptr, tb->struct_name_len)) { return 0; }
    }
    if (ta->type_kind == TYPE_ARRAY || ta->type_kind == TYPE_SLICE) {
        if (ta->elem_type_kind != tb->elem_type_kind) { return 0; }
        if (ta->elem_ptr_depth != tb->elem_ptr_depth) { return 0; }
        if (ta->type_kind == TYPE_ARRAY && ta->array_len != tb->array_len) { return 0; }
        if (!str_eq(ta->struct_name_ptr, ta->struct_name_len, tb->struct_name_ptr, tb->struct_name_len)) { return 0; }
    }
    return 1;
}

func generic_find_binding(bindings: u64, kind: u64, name_ptr: u64, name_len: u64) -> u64 {
    if (bindings == 0) { return 0; }
    var bindings_vec: *Vec<*GenericBinding> = (*Vec<*GenericBinding>)bindings;
    var n: u64 = ((*Vec<*GenericBinding>)bindings_vec)->len();
    for (var i: u64 = 0; i < n; i++) {
        var b: *GenericBinding = ((*Vec<*GenericBinding>)bindings_vec)->get(i);
        if (b->kind == kind && str_eq(b->name_ptr, b->name_len, name_ptr, name_len)) {
            return (u64)b;
        }
    }
    return 0;
}

func generic_bind_type(bindings: u64, name_ptr: u64, name_len: u64, type_ptr: u64) -> u64 {
    var existing: u64 = generic_find_binding(bindings, GENERIC_KIND_TYPE, name_ptr, name_len);
    if (existing != 0) {
        var b: *GenericBinding = (*GenericBinding)existing;
        if (!generic_type_equal(b->type_ptr, type_ptr)) {
            return 1;
        }
        return 0;
    }
    var nb: *GenericBinding = (*GenericBinding)heap_alloc(sizeof(GenericBinding));
    nb->kind = GENERIC_KIND_TYPE;
    nb->name_ptr = name_ptr;
    nb->name_len = name_len;
    nb->type_ptr = type_ptr;
    nb->value = 0;
    ((*Vec<*GenericBinding>)bindings)->push(nb);
    return 0;
}

func generic_bind_value(bindings: u64, name_ptr: u64, name_len: u64, value: u64) -> u64 {
    var existing: u64 = generic_find_binding(bindings, GENERIC_KIND_VALUE, name_ptr, name_len);
    if (existing != 0) {
        var b: *GenericBinding = (*GenericBinding)existing;
        if (b->value != value) {
            return 1;
        }
        return 0;
    }
    var nb: *GenericBinding = (*GenericBinding)heap_alloc(sizeof(GenericBinding));
    nb->kind = GENERIC_KIND_VALUE;
    nb->name_ptr = name_ptr;
    nb->name_len = name_len;
    nb->type_ptr = 0;
    nb->value = value;
    ((*Vec<*GenericBinding>)bindings)->push(nb);
    return 0;
}


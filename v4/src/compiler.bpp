func generic_get_or_create_instance(tpl_ptr: u64, bindings: u64) -> u64 {
    var tpl: *GenericFuncTemplate = (*GenericFuncTemplate)tpl_ptr;
    var base_ptr: u64 = tpl->mangled_ptr;
    var base_len: u64 = tpl->mangled_len;
    if (base_ptr == 0) {
        base_ptr = tpl->name_ptr;
        base_len = tpl->name_len;
    }

    var name_info: *NameInfo = (*NameInfo)generic_mangle_name(base_ptr, base_len, tpl->params_vec, bindings);
    var inst_ptr: u64 = name_info->ptr;
    var inst_len: u64 = name_info->len;

    if (g_generic_instantiations != 0) {
        var exists: u64 = ((*HashMap<u64, u64>)g_generic_instantiations)->get(inst_ptr, inst_len);
        if (exists != 0) { return (u64)name_info; }
    }

    var def_src: u64 = tpl->func_def;
    if (def_src == 0 && tpl->func_sig != 0) { def_src = tpl->func_sig; }
    if (def_src == 0) {
        emit_stderr("[ERROR] Missing generic function body\n", 43);
        panic("Parse error");
    }

    var fn_new: u64 = generic_clone_func(def_src, bindings, inst_ptr, inst_len);
    var sig_new: u64 = generic_clone_func_sig(def_src, bindings, inst_ptr, inst_len);
    ((*Vec<*AstFunc>)g_all_funcs)->push((*AstFunc)fn_new);
    ((*Vec<*AstFunc>)g_all_func_sigs)->push((*AstFunc)sig_new);

    if (g_generic_instantiations == 0) { g_generic_instantiations = HashMap.new<u64, u64>(64); }
    ((*HashMap<u64, u64>)g_generic_instantiations)->put(inst_ptr, inst_len, 1);

    // Map new function to template module
    if (g_func_module_map != 0 && tpl->mangled_ptr != 0) {
        var mod_info: *NameInfo = ((*HashMap<u64, *NameInfo>)g_func_module_map)->get(tpl->mangled_ptr, tpl->mangled_len);
        if (mod_info != 0) {
            ((*HashMap<u64, *NameInfo>)g_func_module_map)->put(inst_ptr, inst_len, mod_info);
        }
    }

    return (u64)name_info;
}

func generic_build_bindings_for_explicit(tpl_ptr: u64, gargs: u64) -> u64 {
    var tpl: *GenericFuncTemplate = (*GenericFuncTemplate)tpl_ptr;
    var bindings: *Vec<*GenericBinding> = Vec.new<*GenericBinding>(4);
    var pn: u64 = ((*Vec<u64>)tpl->params_vec)->len();
    var an: u64 = ((*Vec<u64>)gargs)->len();
    if (pn != an) {
        emit_stderr("[ERROR] Generic argument count mismatch\n", 46);
        panic("Parse error");
    }
    for (var i: u64 = 0; i < pn; i++) {
        var gp: *GenericParam = (*GenericParam)((*Vec<u64>)tpl->params_vec)->get(i);
        var ga: *GenericArg = (*GenericArg)((*Vec<u64>)gargs)->get(i);
        if (gp->kind == GENERIC_KIND_TYPE) {
            if (ga->kind != GENERIC_KIND_TYPE) {
                emit_stderr("[ERROR] Expected type generic argument\n", 47);
                panic("Parse error");
            }
            if (generic_bind_type((u64)bindings, gp->name_ptr, gp->name_len, ga->type_ptr) != 0) {
                emit_stderr("[ERROR] Conflicting generic type bindings\n", 45);
                panic("Parse error");
            }
        } else {
            if (ga->kind != GENERIC_KIND_VALUE) {
                emit_stderr("[ERROR] Expected value generic argument\n", 48);
                panic("Parse error");
            }
            if (generic_bind_value((u64)bindings, gp->name_ptr, gp->name_len, ga->value) != 0) {
                emit_stderr("[ERROR] Conflicting generic value bindings\n", 46);
                panic("Parse error");
            }
        }
    }
    return (u64)bindings;
}


func generic_subst_typeinfo(type_kind: u64, ptr_depth: u64, is_tagged: u64, struct_name_ptr: u64, struct_name_len: u64, tag_layout_ptr: u64, tag_layout_len: u64, elem_type_kind: u64, elem_ptr_depth: u64, array_len: u64, array_len_is_param: u64, array_len_param_ptr: u64, array_len_param_len: u64, bindings: u64) -> u64 {
    if (type_kind == TYPE_GENERIC) {
        if (struct_name_ptr == 0 || struct_name_len == 0) {
            return generic_make_typeinfo_from_fields(TYPE_GENERIC, ptr_depth, is_tagged, struct_name_ptr, struct_name_len, tag_layout_ptr, tag_layout_len, elem_type_kind, elem_ptr_depth, array_len, array_len_is_param, array_len_param_ptr, array_len_param_len);
        }
        var b_ptr: u64 = generic_find_binding(bindings, GENERIC_KIND_TYPE, struct_name_ptr, struct_name_len);
        if (b_ptr == 0) {
            emit_stderr("[ERROR] Missing generic type binding: ", 43);
            emit_stderr(struct_name_ptr, struct_name_len);
            emit_stderr(" len=", 5);
            print_u64(struct_name_len);
            emit_stderr(" ptr=", 5);
            print_u64(struct_name_ptr);
            emit_stderr("\n", 1);
            panic("Parse error");
        }
        var b: *GenericBinding = (*GenericBinding)b_ptr;
        var bt: *TypeInfo = (*TypeInfo)b->type_ptr;
        var merged_ptr_depth: u64 = bt->ptr_depth + ptr_depth;
        return generic_make_typeinfo_from_fields(bt->type_kind, merged_ptr_depth, bt->is_tagged, bt->struct_name_ptr, bt->struct_name_len, bt->tag_layout_ptr, bt->tag_layout_len, bt->elem_type_kind, bt->elem_ptr_depth, bt->array_len, bt->array_len_is_param, bt->array_len_param_ptr, bt->array_len_param_len);
    }

    if (bindings != 0 && type_kind == TYPE_STRUCT && struct_name_ptr != 0 && struct_name_len != 0) {
        var b_ptr2: u64 = generic_find_binding(bindings, GENERIC_KIND_TYPE, struct_name_ptr, struct_name_len);
        if (b_ptr2 != 0) {
            var b2: *GenericBinding = (*GenericBinding)b_ptr2;
            var bt2: *TypeInfo = (*TypeInfo)b2->type_ptr;
            var merged_ptr_depth2: u64 = bt2->ptr_depth + ptr_depth;
            return generic_make_typeinfo_from_fields(bt2->type_kind, merged_ptr_depth2, bt2->is_tagged, bt2->struct_name_ptr, bt2->struct_name_len, bt2->tag_layout_ptr, bt2->tag_layout_len, bt2->elem_type_kind, bt2->elem_ptr_depth, bt2->array_len, bt2->array_len_is_param, bt2->array_len_param_ptr, bt2->array_len_param_len);
        }
    }

    if (type_kind == TYPE_STRUCT && bindings != 0) {
        var base_len: u64 = compiler_find_generic_suffix_index(struct_name_ptr, struct_name_len);
        if (base_len < struct_name_len) {
            var tpl_ptr: u64 = compiler_find_generic_struct_template_by_name(struct_name_ptr, base_len);
            if (tpl_ptr != 0) {
                var name_info: *NameInfo = (*NameInfo)generic_struct_get_or_create_instance(tpl_ptr, bindings);
                return generic_make_typeinfo_from_fields(type_kind, ptr_depth, is_tagged, name_info->ptr, name_info->len, tag_layout_ptr, tag_layout_len, elem_type_kind, elem_ptr_depth, array_len, array_len_is_param, array_len_param_ptr, array_len_param_len);
            }
        }
    }

    if (type_kind == TYPE_ARRAY || type_kind == TYPE_SLICE) {
        var new_array_len: u64 = array_len;
        var new_array_len_is_param: u64 = array_len_is_param;
        var new_array_param_ptr: u64 = array_len_param_ptr;
        var new_array_param_len: u64 = array_len_param_len;
        if (array_len_is_param != 0) {
            var vb_ptr: u64 = generic_find_binding(bindings, GENERIC_KIND_VALUE, array_len_param_ptr, array_len_param_len);
            if (vb_ptr == 0) {
                emit_stderr("[ERROR] Missing generic value binding\n", 40);
                panic("Parse error");
            }
            var vb: *GenericBinding = (*GenericBinding)vb_ptr;
            new_array_len = vb->value;
            new_array_len_is_param = 0;
            new_array_param_ptr = 0;
            new_array_param_len = 0;
        }

        var elem_sub: u64 = generic_subst_typeinfo(elem_type_kind, elem_ptr_depth, 0, struct_name_ptr, struct_name_len, 0, 0, 0, 0, 0, 0, 0, 0, bindings);
        var et: *TypeInfo = (*TypeInfo)elem_sub;
        return generic_make_typeinfo_from_fields(type_kind, ptr_depth, is_tagged, et->struct_name_ptr, et->struct_name_len, tag_layout_ptr, tag_layout_len, et->type_kind, et->ptr_depth, new_array_len, new_array_len_is_param, new_array_param_ptr, new_array_param_len);
    }

    return generic_make_typeinfo_from_fields(type_kind, ptr_depth, is_tagged, struct_name_ptr, struct_name_len, tag_layout_ptr, tag_layout_len, elem_type_kind, elem_ptr_depth, array_len, array_len_is_param, array_len_param_ptr, array_len_param_len);
}

func generic_append_type_repr(v: *Vec<u64>, ti_ptr: u64) -> u64 {
    var ti: *TypeInfo = (*TypeInfo)ti_ptr;
    if (ti->ptr_depth > 0) {
        ((*Vec<u64>)v)->push(112); // 'p'
        generic_vec_append_u64(v, ti->ptr_depth);
        ((*Vec<u64>)v)->push(95); // '_'
        var base: u64 = generic_make_typeinfo_from_fields(ti->type_kind, 0, ti->is_tagged, ti->struct_name_ptr, ti->struct_name_len, ti->tag_layout_ptr, ti->tag_layout_len, ti->elem_type_kind, ti->elem_ptr_depth, ti->array_len, ti->array_len_is_param, ti->array_len_param_ptr, ti->array_len_param_len);
        return generic_append_type_repr(v, base);
    }

    switch (ti->type_kind) {
        case TYPE_U8: generic_vec_append_str(v, "u8", 2); break;
        case TYPE_U16: generic_vec_append_str(v, "u16", 3); break;
        case TYPE_U32: generic_vec_append_str(v, "u32", 3); break;
        case TYPE_U64: generic_vec_append_str(v, "u64", 3); break;
        case TYPE_I64: generic_vec_append_str(v, "i64", 3); break;
        case TYPE_STRUCT:
            ((*Vec<u64>)v)->push(83); // 'S'
            for (var i: u64 = 0; i < ti->struct_name_len; i++) {
                var c: u64 = *(*u8)(ti->struct_name_ptr + i);
                if (c == 47 || c == 46) { c = 95; }
                ((*Vec<u64>)v)->push(c);
            }
            break;
        case TYPE_ARRAY:
            ((*Vec<u64>)v)->push(97); // 'a'
            generic_vec_append_u64(v, ti->array_len);
            ((*Vec<u64>)v)->push(95);
            var elem: u64 = generic_make_typeinfo_from_fields(ti->elem_type_kind, ti->elem_ptr_depth, 0, ti->struct_name_ptr, ti->struct_name_len, 0, 0, 0, 0, 0, 0, 0, 0);
            generic_append_type_repr(v, elem);
            break;
        case TYPE_SLICE:
            ((*Vec<u64>)v)->push(115); // 's'
            ((*Vec<u64>)v)->push(95);
            var elem2: u64 = generic_make_typeinfo_from_fields(ti->elem_type_kind, ti->elem_ptr_depth, 0, ti->struct_name_ptr, ti->struct_name_len, 0, 0, 0, 0, 0, 0, 0, 0);
            generic_append_type_repr(v, elem2);
            break;
        default:
            generic_vec_append_str(v, "t", 1);
            break;
    }
    return 0;
}

func generic_mangle_name(base_ptr: u64, base_len: u64, params_vec: u64, bindings: u64) -> u64 {
    var v: *Vec<u64> = Vec.new<u64>(base_len + 32);
    generic_vec_append_str(v, base_ptr, base_len);
    generic_vec_append_str(v, "__G__", 5);
    var n: u64 = ((*Vec<u64>)params_vec)->len();
    for (var i: u64 = 0; i < n; i++) {
        var gp: *GenericParam = (*GenericParam)((*Vec<u64>)params_vec)->get(i);
        if (gp->kind == GENERIC_KIND_TYPE) {
            ((*Vec<u64>)v)->push(84); // 'T'
            ((*Vec<u64>)v)->push(95);
            var b_ptr: u64 = generic_find_binding(bindings, GENERIC_KIND_TYPE, gp->name_ptr, gp->name_len);
            if (b_ptr == 0) {
                emit_stderr("[ERROR] Missing generic type binding\n", 39);
                panic("Parse error");
            }
            var b: *GenericBinding = (*GenericBinding)b_ptr;
            generic_append_type_repr(v, b->type_ptr);
        } else {
            ((*Vec<u64>)v)->push(86); // 'V'
            ((*Vec<u64>)v)->push(95);
            var b2_ptr: u64 = generic_find_binding(bindings, GENERIC_KIND_VALUE, gp->name_ptr, gp->name_len);
            if (b2_ptr == 0) {
                emit_stderr("[ERROR] Missing generic value binding\n", 40);
                panic("Parse error");
            }
            var b2: *GenericBinding = (*GenericBinding)b2_ptr;
            generic_vec_append_u64(v, b2->value);
        }

        if (i + 1 < n) { ((*Vec<u64>)v)->push(95); }
    }

    var out_len: u64 = ((*Vec<u64>)v)->len();
    var out_ptr: u64 = heap_alloc((out_len + 1) * sizeof(u8));
    for (var i2: u64 = 0; i2 < out_len; i2++) {
        *(*u8)(out_ptr + i2) = ((*Vec<u64>)v)->get(i2);
    }
    *(*u8)(out_ptr + out_len) = 0;
    var info: *NameInfo = (*NameInfo)heap_alloc(sizeof(NameInfoLocal));
    info->ptr = out_ptr;
    info->len = out_len;
    return (u64)info;
}


// Covers: compile-fail check for private method call from outside owner struct
// Expect compile fail: 1
// Expect error contains: private method access denied

struct Gate {
    public value: i64;
}

impl Gate {
    private func hidden(self: *Gate) -> i64 {
        return self.value + 1;
    }

    public func set(self: *Gate, v: i64) {
        self.value = v;
    }
}

func main() -> i64 {
    var g: Gate;
    g.set(5);
    return g.hidden();
}

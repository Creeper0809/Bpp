// Covers: default-private behavior for impl methods without explicit modifier
// Expect compile fail: 1
// Expect error contains: private method access denied

struct AutoPrivate {
    public value: i64;
}

impl AutoPrivate {
    func hidden(self: *AutoPrivate) -> i64 {
        return self.value + 1;
    }
}

func main() -> i64 {
    var x: AutoPrivate;
    x.value = 9;
    return x.hidden();
}

// Covers: generic with struct value type
// Mode: nossa
// Opt: O0
// Expect exit code: 0

struct Pair {
    a: u64;
    b: u64;
}

func id<T>(x: T) -> T { return x; }

func main() -> u64 {
    var p: Pair;
    p.a = 10;
    p.b = 20;
    var q: Pair = id(p);
    if (q.a + q.b != 30) { return 7; }
    return 0;
}

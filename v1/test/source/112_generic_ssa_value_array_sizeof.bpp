// Covers: SSA value generics with sizeof([N]T)
// Mode: ssa
// Opt: O0
// Expect exit code: 0

// Import

func size_of_buf<const N: u64>() -> u64 {
    return sizeof([N]u64);
}

func main() -> u64 {
    // Setup

    // Execution
    var size: u64 = size_of_buf<4>();

    // Assertion
    if (size != 32) { return 13; }
    return 0;
}

// Covers: ABI sret struct return smoke
// Mode: ssa
// Opt: O1
// Expect exit code: 0

// Import

// Setup
struct Pair {
    a: u64;
    b: u64;
}

func make_pair(a: u64, b: u64) -> Pair {
    return Pair { a, b };
}

func main() -> u64 {
    // Execution
    var p: Pair = make_pair(4, 5);
    var total: u64 = p.a + p.b;

    // Assertion
    if (total != 9) { return 1; }
    return 0;
}

// Covers: value generic with local array and pointer
// Mode: nossa
// Opt: O0
// Expect exit code: 0

func first<const N: u64>(buf: *u64) -> u64 {
    return *buf;
}

func main() -> u64 {
    var arr: [4]u64;
    arr[0] = 9;
    var v: u64 = first<4>(&arr[0]);
    if (v != 9) { return 9; }
    return 0;
}

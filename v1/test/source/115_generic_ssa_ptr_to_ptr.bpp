// Covers: SSA generic pointer depth propagation (double pointer)
// Mode: ssa
// Opt: O0
// Expect exit code: 0

// Import

func pass_through<T>(p: **T) -> **T { return p; }

func main() -> u64 {
    // Setup
    var x: u64 = 7;
    var p: *u64 = &x;
    var pp: **u64 = &p;

    // Execution
    var out: **u64 = pass_through<u64>(pp);

    // Assertion
    if (**out != 7) { return 17; }
    return 0;
}

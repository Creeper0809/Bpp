// Covers: ABI function pointer call smoke
// Mode: ssa
// Opt: O1
// Expect exit code: 0

// Import

// Setup
func add(a: u64, b: u64) -> u64 {
    return a + b;
}

func main() -> u64 {
    // Execution
    var fp: u64 = &add;
    var result: u64 = fp(10, 20);

    // Assertion
    if (result != 30) { return 1; }
    return 0;
}

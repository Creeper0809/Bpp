// Covers: SSA generic stress with many instantiations (O1)
// Mode: ssa
// Opt: O1
// Expect exit code: 0

// Import

func id<T>(x: T) -> T { return x; }

func main() -> u64 {
    // Setup
    var a: u64 = 1;
    var b: u32 = 2;
    var c: u16 = 3;
    var d: u8 = 4;
    var e: i64 = 5;

    // Execution
    var r0: u64 = id<u64>(a);
    var r1: u32 = id<u32>(b);
    var r2: u16 = id<u16>(c);
    var r3: u8 = id<u8>(d);
    var r4: i64 = id<i64>(e);

    var sum: u64 = r0 + r1 + r2 + r3 + r4;

    // Assertion
    if (sum != 15) { return 20; }
    return 0;
}

// Covers: value generics + array addr-of under SSA
// Mode: ssa
// Opt: O0
// Expect exit code: 0

func first<const N: u64>(buf: *u64) -> u64 {
    return *buf;
}

func main() -> u64 {
    // Setup
    var arr: [4]u64;
    arr[0] = 9;

    // Execution
    var v: u64 = first<4>(&arr[0]);

    // Assertion
    if (v != 9) { return 11; }
    return 0;
}

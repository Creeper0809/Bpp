// Covers: ABI basic call with 6 integer args
// Mode: ssa
// Opt: O1
// Expect exit code: 0

// Import

// Setup
func add6(a: u64, b: u64, c: u64, d: u64, e: u64, f: u64) -> u64 {
    return a + b + c + d + e + f;
}

func main() -> u64 {
    // Execution
    var result: u64 = add6(1, 2, 3, 4, 5, 6);

    // Assertion
    if (result != 21) { return 1; }
    return 0;
}

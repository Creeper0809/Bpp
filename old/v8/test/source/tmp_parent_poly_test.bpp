struct Parent {
    v: i64;
}

struct Child : Parent {
    x: i64;
}

impl Parent {
    abst func f(self: *Parent) -> i64;
}

impl Child {
    func f(self: *Child) -> i64 {
        return self.v + self.x;
    }
}

func main() -> i64 {
    var c: Child;
    c.v = 2;
    c.x = 5;
    var p: *Parent = (*Parent)&c;
    var r: i64 = p.f();
    if (r != 7) { return 1; }
    return 0;
}

func cg_safe_member_access_expr(node: u64, symtab: *Symtab) -> u64 {
    var member_access: *AstMemberAccess = (*AstMemberAccess)node;
    var object: u64 = member_access.object;
    var member_ptr: u64 = member_access.member_ptr;
    var member_len: u64 = member_access.member_len;
    var parent_ptr: u64 = member_access.parent_ptr;
    var parent_len: u64 = member_access.parent_len;

    var ot: *TypeInfo = get_expr_type_with_symtab((*AstNode)object, symtab);
    if (ot == 0) {
        emit_stderr_len("[ERROR] Safe member access type not found\n", 44);
        return 0;
    }
    if (ot.ptr_depth == 0) {
        emit_stderr_len("[ERROR] Safe member access requires pointer receiver\n", 59);
        return 0;
    }
    if (ot.type_kind != TYPE_STRUCT) {
        emit_stderr_len("[ERROR] Safe member access requires struct pointer\n", 56);
        return 0;
    }

    var label_null: u64 = new_label();
    var label_end: u64 = new_label();

    cg_expr(object);
    if (ot.is_tagged == 1) {
        emit_tagged_mask();
    }
    emit("    test rax, rax\n");
    emit("    je ");
    emit_label(label_null);
    emit_nl();

    if (ot.is_tagged == 1 && ot.tag_layout_ptr != 0) {
        var layout_info: *AstStructDef = get_struct_def(ot.tag_layout_ptr, ot.tag_layout_len);
        if (layout_info == 0) {
            emit("[ERROR] Tagged layout struct not found\n");
            panic("Codegen error");
        }
        var packed_flag: u64 = layout_info.is_packed;
        if (packed_flag == 0) {
            emit("[ERROR] Tagged layout must be packed struct\n");
            panic("Codegen error");
        }
        var total_bits: u64 = get_packed_layout_total_bits(layout_info);
        var field_offset: u64 = get_packed_field_bit_offset(layout_info, member_ptr, member_len);
        var field_width: u64 = get_packed_field_bit_width(layout_info, member_ptr, member_len);
        var start_bit: u64 = 64 - total_bits;
        var shift_bits: u64 = start_bit + field_offset;

        if (shift_bits > 0) {
            emit("    mov rcx, ");
            emit_u64(shift_bits);
            emit_nl();
            emit("    shr rax, cl\n");
        }
        if (field_width < 64) {
            emit_mask_to_rdx(field_width);
            emit("    and rax, rdx\n");
        }
        emit("    jmp ");
        emit_label(label_end);
        emit_nl();
        emit_label_def(label_null);
        emit("    xor eax, eax\n");
        emit_label_def(label_end);
        return 0;
    }

    var struct_def: *AstStructDef = ot.struct_def;
    if (struct_def == 0) {
        emit("    ; ERROR: Struct definition not found for pointer type\n");
        emit("    jmp ");
        emit_label(label_end);
        emit_nl();
        emit_label_def(label_null);
        emit("    xor eax, eax\n");
        emit_label_def(label_end);
        return 0;
    }

    var field_offset2: u64 = 0;
    var field_desc: *FieldDesc = 0;
    var found2: u64 = struct_find_field_desc_scoped(struct_def, parent_ptr, parent_len, member_ptr, member_len, &field_offset2, &field_desc);
    if (found2 == 0) { return 0; }
    if (field_offset2 > 0) {
        emit("    add rax, ");
        emit_u64(field_offset2);
        emit("\n");
    }
    if (field_desc == 0) {
        emit("    jmp ");
        emit_label(label_end);
        emit_nl();
        emit_label_def(label_null);
        emit("    xor eax, eax\n");
        emit_label_def(label_end);
        return 0;
    }
    var fd: *FieldDesc = field_desc;
    if (fd.type_kind == TYPE_STRUCT && fd.ptr_depth == 0) {
        emit("    ; ERROR: Safe member access cannot return struct by value\n");
        emit("    jmp ");
        emit_label(label_end);
        emit_nl();
        emit_label_def(label_null);
        emit("    xor eax, eax\n");
        emit_label_def(label_end);
        return 0;
    }
    if (fd.type_kind == TYPE_ARRAY) {
        emit("    jmp ");
        emit_label(label_end);
        emit_nl();
        emit_label_def(label_null);
        emit("    xor eax, eax\n");
        emit_label_def(label_end);
        return 0;
    }
    if (fd.type_kind == TYPE_SLICE) {
        emit("    mov rax, [rax]\n");
        emit("    jmp ");
        emit_label(label_end);
        emit_nl();
        emit_label_def(label_null);
        emit("    xor eax, eax\n");
        emit_label_def(label_end);
        return 0;
    }
    if (fd.ptr_depth == 0) {
        if (fd.type_kind == TYPE_U8) { emit("    movzx rax, byte [rax]\n"); }
        else if (fd.type_kind == TYPE_U16) { emit("    movzx rax, word [rax]\n"); }
        else if (fd.type_kind == TYPE_U32) { emit("    mov eax, [rax]\n"); }
        else { emit("    mov rax, [rax]\n"); }
    } else {
        emit("    mov rax, [rax]\n");
    }

    emit("    jmp ");
    emit_label(label_end);
    emit_nl();
    emit_label_def(label_null);
    emit("    xor eax, eax\n");
    emit_label_def(label_end);
    return 0;
}

func cg_member_access_expr(node: u64, symtab: *Symtab) -> u64 {
    var member_access: *AstMemberAccess = (*AstMemberAccess)node;
    var object: u64 = member_access.object;
    var member_ptr: u64 = member_access.member_ptr;
    var member_len: u64 = member_access.member_len;
    var parent_ptr: u64 = member_access.parent_ptr;
    var parent_len: u64 = member_access.parent_len;
    
    var ot: *TypeInfo = get_expr_type_with_symtab((*AstNode)object, symtab);
    if (ot != 0) {
        if (ot.ptr_depth > 0 && ot.is_tagged == 1 && ot.tag_layout_ptr != 0) {
            var layout_info: *AstStructDef = get_struct_def(ot.tag_layout_ptr, ot.tag_layout_len);
            if (layout_info == 0) {
                emit("[ERROR] Tagged layout struct not found\n");
                panic("Codegen error");
            }
            var packed_flag: u64 = layout_info.is_packed;
            if (packed_flag == 0) {
                emit("[ERROR] Tagged layout must be packed struct\n");
                panic("Codegen error");
            }
            var total_bits: u64 = get_packed_layout_total_bits(layout_info);
            var field_offset: u64 = get_packed_field_bit_offset(layout_info, member_ptr, member_len);
            var field_width: u64 = get_packed_field_bit_width(layout_info, member_ptr, member_len);
            var start_bit: u64 = 64 - total_bits;
            var shift_bits: u64 = start_bit + field_offset;

            cg_expr(object);
            if (shift_bits > 0) {
                emit("    mov rcx, ");
                emit_u64(shift_bits);
                emit_nl();
                emit("    shr rax, cl\n");
            }
            if (field_width < 64) {
                emit_mask_to_rdx(field_width);
                emit("    and rax, rdx\n");
            }
            return;
        }
        if (ot.ptr_depth > 0 && ot.type_kind == TYPE_STRUCT) {
            cg_expr(object);
            if (ot.is_tagged == 1) {
                emit_tagged_mask();
            }

            var struct_def_ptr: *AstStructDef = ot.struct_def;
            if (struct_def_ptr == 0) {
                emit("    ; ERROR: Struct definition not found for pointer type\n");
                return;
            }

            var field_offset_ptr: u64 = 0;
            var field_desc_ptr: *FieldDesc = 0;
            var found_ptr: u64 = struct_find_field_desc_scoped(struct_def_ptr, parent_ptr, parent_len, member_ptr, member_len, &field_offset_ptr, &field_desc_ptr);
            if (found_ptr == 0) { return; }
            if (field_offset_ptr > 0) {
                emit("    add rax, ");
                emit_u64(field_offset_ptr);
                emit("\n");
            }
            if (field_desc_ptr == 0) { return; }
            var fd_ptr: *FieldDesc = field_desc_ptr;
            if (fd_ptr.type_kind == TYPE_ARRAY) { return; }
            if (fd_ptr.type_kind == TYPE_SLICE) { emit("    mov rax, [rax]\n"); return; }
            if (fd_ptr.ptr_depth == 0) {
                if (fd_ptr.type_kind == TYPE_U8) { emit("    movzx rax, byte [rax]\n"); return; }
                if (fd_ptr.type_kind == TYPE_U16) { emit("    movzx rax, word [rax]\n"); return; }
                if (fd_ptr.type_kind == TYPE_U32) { emit("    mov eax, [rax]\n"); return; }
            }
            emit("    mov rax, [rax]\n");
            return;
        }
        if (ot.ptr_depth == 0 && ot.type_kind == TYPE_STRUCT && ot.struct_def != 0) {
            var struct_info: *AstStructDef = ot.struct_def;
            var packed_flag2: u64 = struct_info.is_packed;
            if (packed_flag2 == 1) {
                var total_bits2: u64 = get_packed_layout_total_bits(struct_info);
                var field_offset2: u64 = get_packed_field_bit_offset(struct_info, member_ptr, member_len);
                var field_width2: u64 = get_packed_field_bit_width(struct_info, member_ptr, member_len);
                var shift_bits2: u64 = field_offset2;
                var size_bytes2: u64 = (total_bits2 + 7) / 8;

                if (total_bits2 > 64 || (field_offset2 + field_width2) > 64) {
                    var byte_offset2: u64 = field_offset2 / 8;
                    var bit_shift2: u64 = field_offset2 % 8;
                    var byte_width2: u64 = (field_width2 + 7) / 8;

                    cg_lvalue(object);
                    if (byte_offset2 > 0) {
                        emit("    add rax, ");
                        emit_u64(byte_offset2);
                        emit("\n");
                    }

                    if (byte_width2 == 1) {
                        emit("    movzx rax, byte [rax]\n");
                    } else if (byte_width2 == 2) {
                        emit("    movzx rax, word [rax]\n");
                    } else if (byte_width2 == 4) {
                        emit("    mov eax, [rax]\n");
                    } else {
                        emit("    mov rax, [rax]\n");
                    }

                    if (bit_shift2 > 0) {
                        emit("    mov rcx, ");
                        emit_u64(bit_shift2);
                        emit_nl();
                        emit("    shr rax, cl\n");
                    }
                    if (field_width2 < 64) {
                        emit_mask_to_rdx(field_width2);
                        emit("    and rax, rdx\n");
                    }
                    return;
                }

                cg_lvalue(object);
                if (size_bytes2 == 1) {
                    emit("    movzx rax, byte [rax]\n");
                } else if (size_bytes2 == 2) {
                    emit("    movzx rax, word [rax]\n");
                } else if (size_bytes2 == 4) {
                    emit("    mov eax, [rax]\n");
                } else {
                    emit("    mov rax, [rax]\n");
                }
                if (shift_bits2 > 0) {
                    emit("    mov rcx, ");
                    emit_u64(shift_bits2);
                    emit_nl();
                    emit("    shr rax, cl\n");
                }
                if (field_width2 < 64) {
                    emit_mask_to_rdx(field_width2);
                    emit("    and rax, rdx\n");
                }
                return;
            }
        }
    }

    var obj_kind: u64 = ast_kind(object);
    
    // Handle ptr.field (object is AST_DEREF)
    if (obj_kind == AST_DEREF) {
        var deref_obj: *AstDeref = (*AstDeref)object;
        var ptr_expr: u64 = deref_obj.operand;
        
        // Evaluate pointer expression to get pointer value
        cg_expr(ptr_expr);
        
        // Get pointer type to find struct_def
        var ti: *TypeInfo = get_expr_type_with_symtab((*AstNode)ptr_expr, symtab);
        if (ti == 0) {
            emit("    ; ERROR: Cannot determine pointer type in arrow operator\n");
            return;
        }

        var base_type: u64 = ti.type_kind;
        var ptr_depth: u64 = ti.ptr_depth;
        if (ptr_depth > 0 && ti.is_tagged == 1) {
            emit_tagged_mask();
        }
        emit("    push rax\n");
        
        if (ptr_depth == 0 || base_type != TYPE_STRUCT) {
            emit("    ; ERROR: Arrow operator requires pointer to struct\n");
            return;
        }
        
        // Get struct_def from pointer's base type
        var struct_def: *AstStructDef = ti.struct_def;
        if (struct_def == 0) {
            emit("    ; ERROR: Struct definition not found for pointer type\n");
            return;
        }
        
        var field_offset: u64 = 0;
        var field_desc: *FieldDesc = 0;
        var found_field: u64 = struct_find_field_desc_scoped(struct_def, parent_ptr, parent_len, member_ptr, member_len, &field_offset, &field_desc);
        if (found_field == 0) { return; }
        
        // Pop pointer value, add field offset
        emit("    pop rax\n");
        if (field_offset > 0) {
            emit("    add rax, ");
            emit_u64(field_offset);
            emit("\n");
        }
        if (field_desc == 0) { return; }
        var fd: *FieldDesc = field_desc;
        if (fd.type_kind == TYPE_ARRAY) { return; }
        if (fd.type_kind == TYPE_SLICE) { emit("    mov rax, [rax]\n"); return; }
        if (fd.ptr_depth == 0) {
            if (fd.type_kind == TYPE_U8) { emit("    movzx rax, byte [rax]\n"); return; }
            if (fd.type_kind == TYPE_U16) { emit("    movzx rax, word [rax]\n"); return; }
            if (fd.type_kind == TYPE_U32) { emit("    mov eax, [rax]\n"); return; }
        }
        emit("    mov rax, [rax]\n");
        return;
    }
    
    // Handle nested member access: outer.inner.field
    if (obj_kind == AST_MEMBER_ACCESS) {
        var nt: *TypeInfo = get_expr_type_with_symtab((*AstNode)object, symtab);
        if (nt == 0) {
            emit("    ; ERROR: Nested member access type not found\n");
            return;
        }
        if (nt.type_kind != TYPE_STRUCT || nt.struct_def == 0) {
            emit("    ; ERROR: Nested member access on non-struct\n");
            return;
        }

        var field_offset_n: u64 = 0;
        var field_desc_n: *FieldDesc = 0;
        var found_n: u64 = struct_find_field_desc_scoped(nt.struct_def, parent_ptr, parent_len, member_ptr, member_len, &field_offset_n, &field_desc_n);
        if (found_n == 0) { return; }

        cg_lvalue(object);
        if (field_offset_n > 0) {
            emit("    add rax, ");
            emit_u64(field_offset_n);
            emit("\n");
        }

        if (field_desc_n != 0) {
            var fdn: *FieldDesc = field_desc_n;
            if (fdn.type_kind == TYPE_ARRAY) { return; }
            if (fdn.type_kind == TYPE_SLICE) { emit("    mov rax, [rax]\n"); return; }
            if (fdn.ptr_depth == 0) {
                if (fdn.type_kind == TYPE_U8) { emit("    movzx rax, byte [rax]\n"); return; }
                if (fdn.type_kind == TYPE_U16) { emit("    movzx rax, word [rax]\n"); return; }
                if (fdn.type_kind == TYPE_U32) { emit("    mov eax, [rax]\n"); return; }
            }
        }
        emit("    mov rax, [rax]\n");
        return;
    }

    // Handle indexed member access: arr[i].field
    if (obj_kind == AST_INDEX) {
        var it: *TypeInfo = get_expr_type_with_symtab((*AstNode)object, symtab);
        if (it == 0) {
            emit("    ; ERROR: Indexed member access type not found\n");
            return;
        }
        if (it.type_kind != TYPE_STRUCT || it.struct_def == 0) {
            emit("    ; ERROR: Indexed member access on non-struct\n");
            return;
        }

        var idx_field_offset: u64 = 0;
        var idx_field_desc: *FieldDesc = 0;
        var found_idx: u64 = struct_find_field_desc_scoped(it.struct_def, parent_ptr, parent_len, member_ptr, member_len, &idx_field_offset, &idx_field_desc);
        if (found_idx == 0) { return; }

        cg_lvalue(object);
        if (idx_field_offset > 0) {
            emit("    add rax, ");
            emit_u64(idx_field_offset);
            emit("\n");
        }

        if (idx_field_desc != 0) {
            var ifd: *FieldDesc = idx_field_desc;
            if (ifd.type_kind == TYPE_ARRAY) { return; }
            if (ifd.type_kind == TYPE_SLICE) { emit("    mov rax, [rax]\n"); return; }
            if (ifd.ptr_depth == 0) {
                if (ifd.type_kind == TYPE_U8) { emit("    movzx rax, byte [rax]\n"); return; }
                if (ifd.type_kind == TYPE_U16) { emit("    movzx rax, word [rax]\n"); return; }
                if (ifd.type_kind == TYPE_U32) { emit("    mov eax, [rax]\n"); return; }
            }
        }
        emit("    mov rax, [rax]\n");
        return;
    }
    
    // Handle obj.field (object is AST_IDENT)
    if (obj_kind != AST_IDENT) {
        emit("    ; ERROR: Member access on non-identifier\n");
        return;
    }
    
    var obj_ident: *AstIdent = (*AstIdent)object;
    var obj_name_ptr: u64 = obj_ident.name_ptr;
    var obj_name_len: u64 = obj_ident.name_len;

    var resolved_ptr: u64 = obj_name_ptr;
    var resolved_len: u64 = obj_name_len;
    var resolved: *NameInfo = resolve_name(obj_name_ptr, obj_name_len);
    if (resolved != 0) {
        resolved_ptr = resolved.ptr;
        resolved_len = resolved.len;
    }
    var ginfo: *GlobalInfo = _cg_find_global_info(resolved_ptr, resolved_len);
    if (ginfo == 0 && (resolved_ptr != obj_name_ptr || resolved_len != obj_name_len)) {
        ginfo = _cg_find_global_info(obj_name_ptr, obj_name_len);
    }
    if (ginfo != 0) {
        var struct_def_g: *AstStructDef = 0;
        var gti: *TypeInfo = ginfo.typeinfo_ptr;
        if (gti != 0) {
            if (gti.struct_def == 0 && gti.struct_name_ptr != 0) {
                gti.struct_def = get_struct_def(gti.struct_name_ptr, gti.struct_name_len);
            }
            struct_def_g = gti.struct_def;
        }
        if (struct_def_g == 0) {
            emit("    ; ERROR: Struct definition not found in global lvalue\n");
            return;
        }
        var field_offset_g: u64 = 0;
        var field_desc_g: *FieldDesc = 0;
        var found_g: u64 = struct_find_field_desc_scoped(struct_def_g, parent_ptr, parent_len, member_ptr, member_len, &field_offset_g, &field_desc_g);
        if (found_g == 0) { return; }
        emit("    lea rax, [rel _gvar_");
        print(resolved_ptr, resolved_len);
        emit("]\n");
        if (field_offset_g > 0) {
            emit("    add rax, ");
            emit_u64(field_offset_g);
            emit("\n");
        }
        if (field_desc_g != 0) {
            var fdg: *FieldDesc = field_desc_g;
            if (fdg.type_kind == TYPE_ARRAY) { return; }
            if (fdg.type_kind == TYPE_SLICE) { emit("    mov rax, [rax]\n"); return; }
            if (fdg.ptr_depth == 0) {
                if (fdg.type_kind == TYPE_U8) { emit("    movzx rax, byte [rax]\n"); return; }
                if (fdg.type_kind == TYPE_U16) { emit("    movzx rax, word [rax]\n"); return; }
                if (fdg.type_kind == TYPE_U32) { emit("    mov eax, [rax]\n"); return; }
            }
        }
        emit("    mov rax, [rax]\n");
        return;
    }
    
    // Get variable info from symtab
    var var_offset: u64 = symtab_find(symtab, obj_name_ptr, obj_name_len);
    var var_ti: *TypeInfo = symtab_get_type(symtab, obj_name_ptr, obj_name_len);
    var type_kind: u64 = var_ti.type_kind;
    
    if (type_kind != TYPE_STRUCT) {
        emit("    ; ERROR: Member access on non-struct type\n");
        return;
    }
    
    // Get struct_def directly from type_info
    var struct_def: *AstStructDef = var_ti.struct_def;
    
    if (struct_def == 0) {
        emit("    ; ERROR: Struct definition not found in type_info\n");
        return;
    }
    
    var field_offset: u64 = 0;
    var field_desc: *FieldDesc = 0;
    var found_local: u64 = struct_find_field_desc_scoped(struct_def, parent_ptr, parent_len, member_ptr, member_len, &field_offset, &field_desc);
    if (found_local == 0) { return; }
    
    // Calculate address: lea rax, [rbp + var_offset + field_offset]
    emit("    lea rax, [rbp");
    var total_offset: u64 = var_offset + field_offset;
    if (total_offset < 0) { emit_i64(total_offset); }
    else { emit("+"); emit_u64(total_offset); }
    emit("]\n");
    
    if (field_desc != 0) {
        var fd3: *FieldDesc = field_desc;
        if (fd3.type_kind == TYPE_ARRAY) { return; }
        if (fd3.type_kind == TYPE_SLICE) { emit("    mov rax, [rax]\n"); return; }
        if (fd3.ptr_depth == 0) {
            if (fd3.type_kind == TYPE_U8) { emit("    movzx rax, byte [rax]\n"); return; }
            if (fd3.type_kind == TYPE_U16) { emit("    movzx rax, word [rax]\n"); return; }
            if (fd3.type_kind == TYPE_U32) { emit("    mov eax, [rax]\n"); return; }
        }
    }
    emit("    mov rax, [rax]\n");
}

// ============================================
// Binary Expression
// ============================================

func cg_binary_expr(node: u64, symtab: *Symtab) -> u64 {
    var binary: *AstBinary = (*AstBinary)node;
    var op: u64 = binary.op;
    var left: u64 = binary.left;
    var right: u64 = binary.right;

    // Short-circuit evaluation for && and ||
    if (op == TOKEN_ANDAND) {
        var l_false: u64 = new_label();
        var l_end: u64 = new_label();

        cg_expr(left);
        emit("    test rax, rax\n");
        emit("    jz ");
        emit_label(l_false);
        emit("\n");

        cg_expr(right);
        emit("    test rax, rax\n");
        emit("    setne al\n");
        emit("    movzx rax, al\n");
        emit("    jmp ");
        emit_label(l_end);
        emit("\n");

        emit_label_def(l_false);
        emit("    xor eax, eax\n");
        emit_label_def(l_end);
        return;
    }

    if (op == TOKEN_OROR) {
        var l_true: u64 = new_label();
        var l_end: u64 = new_label();

        cg_expr(left);
        emit("    test rax, rax\n");
        emit("    jnz ");
        emit_label(l_true);
        emit("\n");

        cg_expr(right);
        emit("    test rax, rax\n");
        emit("    setne al\n");
        emit("    movzx rax, al\n");
        emit("    jmp ");
        emit_label(l_end);
        emit("\n");

        emit_label_def(l_true);
        emit("    mov eax, 1\n");
        emit_label_def(l_end);
        return;
    }
    
    // Standard binary: evaluate both sides
    cg_expr(left);
    emit("    push rax\n");
    cg_expr(right);
    emit("    mov rbx, rax\n");
    emit("    pop rax\n");

    var left_ti: *TypeInfo = get_expr_type_with_symtab((*AstNode)left, symtab);
    var right_ti: *TypeInfo = get_expr_type_with_symtab((*AstNode)right, symtab);
    var is_float: u64 = 0;
    if (left_ti != 0 && right_ti != 0) {
        if (left_ti.ptr_depth == 0 && right_ti.ptr_depth == 0 && left_ti.type_kind == TYPE_F64 && right_ti.type_kind == TYPE_F64) {
            is_float = 1;
        }
    }
    if (left_ti != 0 && right_ti != 0) {
        var l_is_f: u64 = (left_ti.ptr_depth == 0 && left_ti.type_kind == TYPE_F64);
        var r_is_f: u64 = (right_ti.ptr_depth == 0 && right_ti.type_kind == TYPE_F64);
        if (l_is_f != r_is_f) {
            emit_stderr_len("[ERROR] Mixed float/integer binary operation is not supported\n", 70);
            panic("Codegen error");
        }
    }
    if (is_float != 0) {
        emit("    movq xmm0, rax\n");
        emit("    movq xmm1, rbx\n");
        if (op == TOKEN_PLUS) { emit("    addsd xmm0, xmm1\n"); }
        else if (op == TOKEN_MINUS) { emit("    subsd xmm0, xmm1\n"); }
        else if (op == TOKEN_STAR) { emit("    mulsd xmm0, xmm1\n"); }
        else if (op == TOKEN_SLASH) { emit("    divsd xmm0, xmm1\n"); }
        else if (op == TOKEN_LT || op == TOKEN_GT || op == TOKEN_LTEQ || op == TOKEN_GTEQ || op == TOKEN_EQEQ || op == TOKEN_BANGEQ) {
            emit("    ucomisd xmm0, xmm1\n");
            if (op == TOKEN_LT) { emit("    setb al\n"); }
            else if (op == TOKEN_GT) { emit("    seta al\n"); }
            else if (op == TOKEN_LTEQ) { emit("    setbe al\n"); }
            else if (op == TOKEN_GTEQ) { emit("    setae al\n"); }
            else if (op == TOKEN_EQEQ) { emit("    sete al\n"); }
            else if (op == TOKEN_BANGEQ) { emit("    setne al\n"); }
            emit("    movzx rax, al\n");
            return;
        } else {
            emit_stderr_len("[ERROR] Unsupported float operator\n", 36);
            panic("Codegen error");
        }
        emit("    movq rax, xmm0\n");
        return;
    }
    
    // Pointer arithmetic scaling
    var left_ti2: *TypeInfo = left_ti;
    if (left_ti2 == 0) { left_ti2 = typeinfo_make(TYPE_I64, 0); }
    var ptr_depth: u64 = left_ti2.ptr_depth;
    var use_signed: u64 = 0;
    if (left_ti2.type_kind == TYPE_I64 && left_ti2.ptr_depth == 0) {
        use_signed = 1;
    }
    
    if (ptr_depth > 0) {
        if (op == TOKEN_PLUS || op == TOKEN_MINUS) {
            var psize: u64 = get_pointee_size(left_ti2.type_kind, ptr_depth);
            if (psize > 1) {
                emit("    imul rbx, ");
                emit_u64(psize);
                emit_nl();
            }
        }
    }
    
    // Arithmetic operators
    if (op == TOKEN_PLUS) { emit("    add rax, rbx\n"); }
    else if (op == TOKEN_MINUS) { emit("    sub rax, rbx\n"); }
    else if (op == TOKEN_STAR) { emit("    imul rax, rbx\n"); }
    else if (op == TOKEN_SLASH) {
        if (use_signed == 1) {
            emit("    cqo\n");
            emit("    idiv rbx\n");
        } else {
            emit("    xor rdx, rdx\n");
            emit("    div rbx\n");
        }
    }
    else if (op == TOKEN_PERCENT) {
        if (use_signed == 1) {
            emit("    cqo\n");
            emit("    idiv rbx\n");
        } else {
            emit("    xor rdx, rdx\n");
            emit("    div rbx\n");
        }
        emit("    mov rax, rdx\n");
    }
    // Bitwise operators
    else if (op == TOKEN_CARET) { emit("    xor rax, rbx\n"); }
    else if (op == TOKEN_AMPERSAND) { emit("    and rax, rbx\n"); }
    else if (op == TOKEN_PIPE) { emit("    or rax, rbx\n"); }
    // Shift operators
    else if (op == TOKEN_LSHIFT) { 
        emit("    mov rcx, rbx\n");
        emit("    shl rax, cl\n");
    }
    else if (op == TOKEN_RSHIFT) {
        emit("    mov rcx, rbx\n");
        emit("    shr rax, cl\n");
    }
    // Comparison operators
    else if (op == TOKEN_LT) {
        emit("    cmp rax, rbx\n");
        emit("    setl al\n");
        emit("    movzx rax, al\n");
    }
    else if (op == TOKEN_GT) {
        emit("    cmp rax, rbx\n");
        emit("    setg al\n");
        emit("    movzx rax, al\n");
    }
    else if (op == TOKEN_LTEQ) {
        emit("    cmp rax, rbx\n");
        emit("    setle al\n");
        emit("    movzx rax, al\n");
    }
    else if (op == TOKEN_GTEQ) {
        emit("    cmp rax, rbx\n");
        emit("    setge al\n");
        emit("    movzx rax, al\n");
    }
    else if (op == TOKEN_EQEQ) {
        emit("    cmp rax, rbx\n");
        emit("    sete al\n");
        emit("    movzx rax, al\n");
    }
    else if (op == TOKEN_BANGEQ) {
        emit("    cmp rax, rbx\n");
        emit("    setne al\n");
        emit("    movzx rax, al\n");
    }
}

// ============================================
// LValue Codegen
// ============================================

func cg_lvalue(node: u64) -> u64 {
    var kind: u64 = ast_kind(node);
    var symtab: *Symtab = emitter_get_symtab();
    
    if (kind == AST_IDENT) {
        var ident: *AstIdent = (*AstIdent)node;
        var name_ptr: u64 = ident.name_ptr;
        var name_len: u64 = ident.name_len;

        var resolved_ptr: u64 = name_ptr;
        var resolved_len: u64 = name_len;
        var resolved: *NameInfo = resolve_name(name_ptr, name_len);
        if (resolved != 0) {
            resolved_ptr = resolved.ptr;
            resolved_len = resolved.len;
        }

        if (is_global_var(resolved_ptr, resolved_len)) {
            emit("    lea rax, [rel _gvar_");
            print(resolved_ptr, resolved_len);
            emit("]\n");
            return;
        }
        
        var offset: u64 = symtab_find(symtab, name_ptr, name_len);
        
        emit("    lea rax, [rbp");
        if (offset < 0) { emit_i64(offset); }
        else { emit("+"); emit_u64(offset); }
        emit("]\n");
        return;
    }

    if (kind == AST_CAST) {
        var cast_node: *AstCast = (*AstCast)node;
        cg_lvalue(cast_node.expr);
        return;
    }
    
    if (kind == AST_DEREF) {
        var deref: *AstDeref = (*AstDeref)node;
        var operand: u64 = deref.operand;
        cg_expr(operand);
        var ti: *TypeInfo = get_expr_type_with_symtab((*AstNode)operand, symtab);
        if (ti != 0) {
            if (ti.ptr_depth > 0 && ti.is_tagged == 1) {
                emit_tagged_mask();
            }
        }
        return;
    }
    
    if (kind == AST_DEREF8) {
        var deref8: *AstDeref8 = (*AstDeref8)node;
        var operand: u64 = deref8.operand;
        cg_expr(operand);
        var ti2: *TypeInfo = get_expr_type_with_symtab((*AstNode)operand, symtab);
        if (ti2 != 0) {
            if (ti2.ptr_depth > 0 && ti2.is_tagged == 1) {
                emit_tagged_mask();
            }
        }
        return;
    }

    if (kind == AST_INDEX) {
        cg_index_addr(node, symtab);
        return;
    }
    
    if (kind == AST_MEMBER_ACCESS) {
        cg_member_access_lvalue(node, symtab);
        return;
    }

    if (kind == AST_SAFE_MEMBER_ACCESS) {
        emit_stderr_len("[ERROR] Safe member access cannot be used as lvalue\n", 61);
        return;
    }
}

// ============================================
// Member Access LValue
// ============================================

func cg_member_access_lvalue(node: u64, symtab: *Symtab) -> u64 {
    var member_access: *AstMemberAccess = (*AstMemberAccess)node;
    var object: u64 = member_access.object;
    var member_ptr: u64 = member_access.member_ptr;
    var member_len: u64 = member_access.member_len;
    var parent_ptr: u64 = member_access.parent_ptr;
    var parent_len: u64 = member_access.parent_len;

    var obj_ti: *TypeInfo = get_expr_type_with_symtab((*AstNode)object, symtab);
    if (obj_ti != 0 && obj_ti.ptr_depth > 0 && obj_ti.type_kind == TYPE_STRUCT) {
        cg_expr(object);
        if (obj_ti.is_tagged == 1) {
            emit_tagged_mask();
        }
        var struct_def_ptr: *AstStructDef = obj_ti.struct_def;
        if (struct_def_ptr == 0) {
            emit("    ; ERROR: Struct definition not found for pointer type\n");
            return;
        }
        var field_offset_ptr: u64 = 0;
        var field_desc_ptr: *FieldDesc = 0;
        var found_ptr: u64 = struct_find_field_desc_scoped(struct_def_ptr, parent_ptr, parent_len, member_ptr, member_len, &field_offset_ptr, &field_desc_ptr);
        if (found_ptr == 0) { return; }
        if (field_offset_ptr > 0) {
            emit("    add rax, ");
            emit_u64(field_offset_ptr);
            emit("\n");
        }
        return;
    }
    
    var obj_kind: u64 = ast_kind(object);
    
    // Handle ptr.field (object is AST_DEREF)
    if (obj_kind == AST_DEREF) {
        var deref_obj: *AstDeref = (*AstDeref)object;
        var ptr_expr: u64 = deref_obj.operand;
        
        // Evaluate pointer expression to get pointer value
        cg_expr(ptr_expr);
        
        // Get pointer type to find struct_def
        var ti: *TypeInfo = get_expr_type_with_symtab((*AstNode)ptr_expr, symtab);
        if (ti == 0) {
            emit("    ; ERROR: Cannot determine pointer type in arrow operator\n");
            return;
        }

        var base_type: u64 = ti.type_kind;
        var ptr_depth: u64 = ti.ptr_depth;
        if (ptr_depth > 0 && ti.is_tagged == 1) {
            emit_tagged_mask();
        }
        emit("    push rax\n");
        
        if (ptr_depth == 0 || base_type != TYPE_STRUCT) {
            emit("    ; ERROR: Arrow operator requires pointer to struct\n");
            return;
        }
        
        // Get struct_def from pointer's base type
        var struct_def: *AstStructDef = ti.struct_def;
        if (struct_def == 0) {
            emit("    ; ERROR: Struct definition not found for pointer type\n");
            return;
        }
        
        var field_offset: u64 = 0;
        var field_desc: *FieldDesc = 0;
        var found_field: u64 = struct_find_field_desc_scoped(struct_def, parent_ptr, parent_len, member_ptr, member_len, &field_offset, &field_desc);
        if (found_field == 0) { return; }
        
        // Pop pointer value and add field offset
        emit("    pop rax\n");
        if (field_offset > 0) {
            emit("    add rax, ");
            emit_u64(field_offset);
            emit("\n");
        }
        return;
    }
    
    // Handle nested member access: outer.inner.field (lvalue)
    if (obj_kind == AST_MEMBER_ACCESS) {
        // Recursively get the address of the nested object
        cg_lvalue(object);
        emit("    push rax\n");
        
        // Get the type of the nested object
        var obj_lv_ti: *TypeInfo = get_expr_type_with_symtab((*AstNode)object, symtab);
        if (obj_lv_ti == 0) {
            emit("    ; ERROR: Cannot determine type of nested member in lvalue\n");
            return;
        }
        var base_type: u64 = obj_lv_ti.type_kind;
        if (base_type != TYPE_STRUCT) {
            emit("    ; ERROR: Nested member access on non-struct in lvalue\n");
            return;
        }
        
        var struct_def: *AstStructDef = obj_lv_ti.struct_def;
        if (struct_def == 0) {
            emit("    ; ERROR: Struct definition not found for nested lvalue\n");
            return;
        }
        
        var field_offset: u64 = 0;
        var field_desc: *FieldDesc = 0;
        var found_field2: u64 = struct_find_field_desc_scoped(struct_def, parent_ptr, parent_len, member_ptr, member_len, &field_offset, &field_desc);
        if (found_field2 == 0) { return; }
        
        // Pop base address and add field offset
        emit("    pop rax\n");
        if (field_offset > 0) {
            emit("    add rax, ");
            emit_u64(field_offset);
            emit("\n");
        }
        return;
    }

    // Handle indexed member access: arr[i].field (lvalue)
    if (obj_kind == AST_INDEX) {
        cg_lvalue(object);
        emit("    push rax\n");

        var idx_ti: *TypeInfo = get_expr_type_with_symtab((*AstNode)object, symtab);
        if (idx_ti == 0) {
            emit("    ; ERROR: Cannot determine type of indexed member in lvalue\n");
            return;
        }
        if (idx_ti.type_kind != TYPE_STRUCT) {
            emit("    ; ERROR: Indexed member access on non-struct in lvalue\n");
            return;
        }
        var idx_struct_def: *AstStructDef = idx_ti.struct_def;
        if (idx_struct_def == 0) {
            emit("    ; ERROR: Struct definition not found for indexed lvalue\n");
            return;
        }

        var idx_field_offset: u64 = 0;
        var idx_field_desc: *FieldDesc = 0;
        var found_idx: u64 = struct_find_field_desc_scoped(idx_struct_def, parent_ptr, parent_len, member_ptr, member_len, &idx_field_offset, &idx_field_desc);
        if (found_idx == 0) { return; }
        emit("    pop rax\n");
        if (idx_field_offset > 0) {
            emit("    add rax, ");
            emit_u64(idx_field_offset);
            emit("\n");
        }
        return;
    }
    
    // Handle obj.field (object is AST_IDENT)
    if (obj_kind != AST_IDENT) {
        emit("    ; ERROR: Member access on non-identifier in lvalue\n");
        return;
    }
    
    var obj_ident: *AstIdent = (*AstIdent)object;
    var obj_name_ptr: u64 = obj_ident.name_ptr;
    var obj_name_len: u64 = obj_ident.name_len;

    var resolved_ptr: u64 = obj_name_ptr;
    var resolved_len: u64 = obj_name_len;
    var resolved: *NameInfo = resolve_name(obj_name_ptr, obj_name_len);
    if (resolved != 0) {
        resolved_ptr = resolved.ptr;
        resolved_len = resolved.len;
    }
    var ginfo: *GlobalInfo = _cg_find_global_info(resolved_ptr, resolved_len);
    if (ginfo == 0 && (resolved_ptr != obj_name_ptr || resolved_len != obj_name_len)) {
        ginfo = _cg_find_global_info(obj_name_ptr, obj_name_len);
    }
    if (ginfo != 0) {
        var struct_def_g: *AstStructDef = 0;
        var gti: *TypeInfo = ginfo.typeinfo_ptr;
        if (gti != 0) {
            if (gti.struct_def == 0 && gti.struct_name_ptr != 0) {
                gti.struct_def = get_struct_def(gti.struct_name_ptr, gti.struct_name_len);
            }
            struct_def_g = gti.struct_def;
        }
        if (struct_def_g == 0) {
            emit("    ; ERROR: Struct definition not found in global lvalue\n");
            return;
        }
        var field_offset_g: u64 = 0;
        var field_desc_g: *FieldDesc = 0;
        var found_g: u64 = struct_find_field_desc_scoped(struct_def_g, parent_ptr, parent_len, member_ptr, member_len, &field_offset_g, &field_desc_g);
        if (found_g == 0) { return; }
        emit("    lea rax, [rel _gvar_");
        print(resolved_ptr, resolved_len);
        emit("]\n");
        if (field_offset_g > 0) {
            emit("    add rax, ");
            emit_u64(field_offset_g);
            emit("\n");
        }
        return;
    }
    
    // Get variable info from symtab
    var var_offset: u64 = symtab_find(symtab, obj_name_ptr, obj_name_len);
    var var_type: *TypeInfo = symtab_get_type(symtab, obj_name_ptr, obj_name_len);
    
    // Get struct_def directly from type_info
    var struct_def: *AstStructDef = 0;
    if (var_type != 0) {
        struct_def = var_type.struct_def;
    }
    
    if (struct_def == 0) {
        emit("    ; ERROR: Struct definition not found in lvalue\n");
        return;
    }
    
    var field_offset: u64 = 0;
    var field_desc_l: *FieldDesc = 0;
    var found_l: u64 = struct_find_field_desc_scoped(struct_def, parent_ptr, parent_len, member_ptr, member_len, &field_offset, &field_desc_l);
    if (found_l == 0) { return; }
    
    // Calculate address: lea rax, [rbp + var_offset + field_offset]
    emit("    lea rax, [rbp");
    var total_offset: u64 = var_offset + field_offset;
    if (total_offset < 0) { emit_i64(total_offset); }
    else { emit("+"); emit_u64(total_offset); }
    emit("]\n");
}

// ============================================
// Method Call Code Generation
// ============================================

func cg_safe_method_call(node: u64, symtab: *Symtab) -> u64 {
    var method_call: *AstMethodCall = (*AstMethodCall)node;
    var receiver: u64 = method_call.receiver;
    var method_ptr: u64 = method_call.method_ptr;
    var method_len: u64 = method_call.method_len;
    var args: *Vec<*AstNode> = method_call.args_vec;
    var nargs: u64 = 0;
    if (args != 0) { nargs = args.len(); }

    var recv_ti: *TypeInfo = get_expr_type_with_symtab((*AstNode)receiver, symtab);
    if (recv_ti == 0) {
        emit_stderr_len("[ERROR] Safe method call receiver type not found\n", 56);
        return 0;
    }
    if (recv_ti.type_kind == TYPE_TRAIT || compiler_is_trait_name(recv_ti.struct_name_ptr, recv_ti.struct_name_len) != 0) {
        if (recv_ti.ptr_depth == 0) {
            emit_stderr_len("[ERROR] Safe trait method call requires pointer receiver\n", 65);
            return 0;
        }
        var method_index: u64 = compiler_find_trait_method_index(recv_ti.struct_name_ptr, recv_ti.struct_name_len, method_ptr, method_len);
        if (method_index == 9223372036854775807) {
            emit_stderr_len("[ERROR] Trait method not found: ", 38);
            emit_stderr_len(method_ptr, method_len);
            emit_stderr_len("\n", 1);
            return 0;
        }

        var label_null_t: u64 = new_label();
        var label_end_t: u64 = new_label();

        cg_expr(receiver);
        if (recv_ti.is_tagged == 1) {
            emit_tagged_mask();
        }
        emit("    test rax, rax\n");
        emit("    je ");
        emit_label(label_null_t);
        emit_nl();

        var tmp_offset: u64 = cg_alloc_tmp_struct(symtab, 8);
        emit("    mov [rbp");
        if (tmp_offset < 0) { emit_i64(tmp_offset); }
        else { emit("+"); emit_u64(tmp_offset); }
        emit("], rax\n");

        var total_arg_words_t: u64 = 0;
        if (nargs != 0) {
            var i_t: u64 = nargs;
            while (i_t > 0) {
                i_t = i_t - 1;
                var arg_t: *AstNode = args.get(i_t);
                total_arg_words_t = total_arg_words_t + _cg_sysv_push_call_arg(arg_t, symtab);
            }
        }

        emit("    mov rax, [rbp");
        if (tmp_offset < 0) { emit_i64(tmp_offset); }
        else { emit("+"); emit_u64(tmp_offset); }
        emit("]\n");
        emit("    push rax\n");
        total_arg_words_t = total_arg_words_t + 1;

        var stack_words_t: u64 = _cg_sysv_pop_arg_regs(total_arg_words_t);
        emit("    mov rax, [rdi]\n");
        if (method_index != 0) {
            emit("    add rax, ");
            emit_u64(method_index * 8);
            emit_nl();
        }
        emit("    mov rax, [rax]\n");
        emit("    call rax\n");

        if (stack_words_t > 0) {
            emit("    add rsp, ");
            emit_u64(stack_words_t * 8);
            emit_nl();
        }

        emit("    jmp ");
        emit_label(label_end_t);
        emit_nl();

        emit_label_def(label_null_t);
        emit("    xor eax, eax\n");
        emit_label_def(label_end_t);
        return 0;
    }
    if (recv_ti.ptr_depth == 0) {
        emit_stderr_len("[ERROR] Safe method call requires pointer receiver\n", 59);
        return 0;
    }
    if (recv_ti.type_kind != TYPE_STRUCT) {
        emit_stderr_len("[ERROR] Safe method call requires struct pointer\n", 56);
        return 0;
    }

    var name_info: *NameInfo = compiler_build_method_name(recv_ti.struct_name_ptr, recv_ti.struct_name_len, method_ptr, method_len);
    var resolved_ptr: u64 = name_info.ptr;
    var resolved_len: u64 = name_info.len;
    var resolved: *NameInfo = resolve_name(name_info.ptr, name_info.len);
    if (resolved != 0) {
        resolved_ptr = resolved.ptr;
        resolved_len = resolved.len;
    }

    var fn: *AstFunc = typeinfo_find_func(resolved_ptr, resolved_len);
    var ret_type: u64 = TYPE_I64;
    var ret_ptr_depth: u64 = 0;
    if (fn != 0) {
        ret_type = fn.ret_type;
        ret_ptr_depth = fn.ret_ptr_depth;
        if (ret_type == TYPE_STRUCT && ret_ptr_depth == 0) {
            emit_stderr_len("[ERROR] Safe method call cannot return struct by value\n", 69);
            return 0;
        }
    }

    var label_null: u64 = new_label();
    var label_end: u64 = new_label();

    cg_expr(receiver);
    if (recv_ti.is_tagged == 1) {
        emit_tagged_mask();
    }
    emit("    test rax, rax\n");
    emit("    je ");
    emit_label(label_null);
    emit_nl();

    var tmp_offset: u64 = cg_alloc_tmp_struct(symtab, 8);
    emit("    mov [rbp");
    if (tmp_offset < 0) { emit_i64(tmp_offset); }
    else { emit("+"); emit_u64(tmp_offset); }
    emit("], rax\n");

    var total_arg_words: u64 = 0;
    if (nargs != 0) {
        var i: u64 = nargs;
        while (i > 0) {
            i = i - 1;
            var arg: *AstNode = args.get(i);
            total_arg_words = total_arg_words + _cg_sysv_push_call_arg(arg, symtab);
        }
    }

    emit("    mov rax, [rbp");
    if (tmp_offset < 0) { emit_i64(tmp_offset); }
    else { emit("+"); emit_u64(tmp_offset); }
    emit("]\n");
    emit("    push rax\n");
    total_arg_words = total_arg_words + 1;

    var stack_words: u64 = _cg_sysv_pop_arg_regs(total_arg_words);
    emit("    call ");
    print(resolved_ptr, resolved_len);
    emit_nl();

    if (stack_words > 0) {
        emit("    add rsp, ");
        emit_u64(stack_words * 8);
        emit_nl();
    }

    emit("    jmp ");
    emit_label(label_end);
    emit_nl();

    emit_label_def(label_null);
    if (ret_type != TYPE_VOID) {
        emit("    xor eax, eax\n");
    }
    emit_label_def(label_end);
    return 0;
}

func cg_method_call(node: u64, symtab: *Symtab) -> u64 {
    var method_call: *AstMethodCall = (*AstMethodCall)node;
    var receiver: u64 = method_call.receiver;
    var method_ptr: u64 = method_call.method_ptr;
    var method_len: u64 = method_call.method_len;
    var args: *Vec<*AstNode> = method_call.args_vec;
    var nargs: u64 = 0;
    if (args != 0) { nargs = args.len(); }
    var total_arg_words: u64 = 0;

    var recv_ti: *TypeInfo = get_expr_type_with_symtab((*AstNode)receiver, symtab);
    var recv_is_ptr: u64 = 0;
    if (recv_ti != 0 && recv_ti.ptr_depth > 0) {
        recv_is_ptr = 1;
    }

    if (recv_ti != 0 && (recv_ti.type_kind == TYPE_TRAIT || compiler_is_trait_name(recv_ti.struct_name_ptr, recv_ti.struct_name_len) != 0)) {
        var method_index: u64 = compiler_find_trait_method_index(recv_ti.struct_name_ptr, recv_ti.struct_name_len, method_ptr, method_len);
        if (method_index == 9223372036854775807) {
            emit_stderr_len("[ERROR] Trait method not found: ", 38);
            emit_stderr_len(method_ptr, method_len);
            emit_stderr_len("\n", 1);
            panic("Codegen error");
        }

        if (nargs != 0) {
            var i_t2: u64 = nargs;
            while (i_t2 > 0) {
                i_t2 = i_t2 - 1;
                var arg_t2: *AstNode = args.get(i_t2);
                total_arg_words = total_arg_words + _cg_sysv_push_call_arg(arg_t2, symtab);
            }
        }

        cg_expr(receiver);
        if (recv_ti.is_tagged == 1) {
            emit_tagged_mask();
        }
        emit("    push rax\n");
        total_arg_words = total_arg_words + 1;

        var stack_words_t2: u64 = _cg_sysv_pop_arg_regs(total_arg_words);
        emit("    mov rax, [rdi]\n");
        if (method_index != 0) {
            emit("    add rax, ");
            emit_u64(method_index * 8);
            emit_nl();
        }
        emit("    mov rax, [rax]\n");
        emit("    call rax\n");

        if (stack_words_t2 > 0) {
            emit("    add rsp, ");
            emit_u64(stack_words_t2 * 8);
            emit_nl();
        }
        return 0;
    }
    
    // Push user args (in reverse order)
    if (nargs != 0) {
        var i: u64 = nargs;
        while (i > 0) {
            i = i - 1;
            var arg: *AstNode = args.get(i);
            total_arg_words = total_arg_words + _cg_sysv_push_call_arg(arg, symtab);
        }
    }
    
    // Push receiver address (first arg = self)
    if (recv_is_ptr != 0) {
        cg_expr(receiver);
        if (recv_ti.is_tagged == 1) {
            emit_tagged_mask();
        }
    } else {
        cg_lvalue(receiver);
    }
    emit("    push rax\n");
    total_arg_words = total_arg_words + 1;
    
    // Get receiver type to determine struct name
    var type_info: *TypeInfo = recv_ti;
    if (type_info == 0 || type_info.struct_name_ptr == 0 || type_info.struct_name_len == 0) {
        if (ast_kind(receiver) == AST_IDENT) {
            var rid: *AstIdent = (*AstIdent)receiver;
            var ti_alt: *TypeInfo = symtab_get_type(symtab, rid.name_ptr, rid.name_len);
            if (ti_alt != 0) { type_info = ti_alt; }
        }
    }
    if (type_info == 0 || type_info.struct_name_ptr == 0 || type_info.struct_name_len == 0) {
        emit_stderr_len("[ERROR] Method call receiver type missing struct name\n", 55);
        panic("Codegen error");
    }
    var struct_name_ptr: u64 = type_info.struct_name_ptr;
    var struct_name_len: u64 = type_info.struct_name_len;
    
    // Call StructName_method()
    var name_info: *NameInfo = compiler_build_method_name(struct_name_ptr, struct_name_len, method_ptr, method_len);
    var resolved_ptr: u64 = name_info.ptr;
    var resolved_len: u64 = name_info.len;
    var resolved: *NameInfo = resolve_name(name_info.ptr, name_info.len);
    if (resolved != 0) {
        resolved_ptr = resolved.ptr;
        resolved_len = resolved.len;
    }

    var stack_words: u64 = _cg_sysv_pop_arg_regs(total_arg_words);

    emit("    call ");
    print(resolved_ptr, resolved_len);
    emit_nl();
    
    // Clean up stack (receiver + user args)
    if (stack_words > 0) {
        emit("    add rsp, ");
        emit_u64(stack_words * 8);
        emit_nl();
    }
}

// Covers: automatic super constructor chaining when child constructor omits explicit super.constructor()
// Expect exit code: 0

struct BaseCtor {
    public v: i64;
}

impl BaseCtor {
    public constructor() {
        self.v = 10;
    }
}

struct ChildCtor : BaseCtor {
    public x: i64;
}

impl ChildCtor {
    public constructor() {
        self.x = 7;
    }
}

func main() -> i64 {
    var c: ChildCtor;
    c.constructor();
    if (c.v != 10) { return 1; }
    if (c.x != 7) { return 2; }
    return 0;
}

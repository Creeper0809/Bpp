// Covers: abstract override works without explicit @[override] (annotation is opt-in)
// Expect exit code: 0

struct ParentMissing {
    public v: i64;
}

impl ParentMissing {
    public abst func calc(self: *ParentMissing) -> i64;
}

struct ChildMissing : ParentMissing {
    public x: i64;
}

impl ChildMissing {
    public func calc(self: *ChildMissing) -> i64 {
        return self.v + self.x;
    }
}

func main() -> i64 {
    var c: ChildMissing;
    c.v = 9;
    c.x = 4;
    if (c.calc() != 13) { return 1; }
    return 0;
}

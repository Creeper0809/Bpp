// Covers: explicit override annotation on inherited method
// Expect exit code: 0

struct Base {
    public value: i64;
}

impl Base {
    public func inc(self: *Base, d: i64) -> i64 {
        self.value = self.value + d;
        return self.value;
    }
}

struct Child : Base {
    public bonus: i64;
}

impl Child {
    @[override]
    public func inc(self: *Child, d: i64) -> i64 {
        var base_v: i64 = super.inc(d);
        return base_v + self.bonus;
    }
}

func main() -> i64 {
    var c: Child;
    c.value = 3;
    c.bonus = 7;

    if (c.inc(2) != 12) { return 1; }
    return 0;
}

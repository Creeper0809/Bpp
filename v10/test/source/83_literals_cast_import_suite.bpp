// Covers: char const literal parse, f64 cast expression, and std.char import in one runtime case
// Expect exit code: 0

import std.char;

const A = 'A';
const NL = '\n';
const TAB = '\t';
const CR = '\r';
const BS = '\\';
const SQ = '\'';

func main() -> u64 {
    if (A != 65) { return 1; }
    if (NL != 10) { return 2; }
    if (TAB != 9) { return 3; }
    if (CR != 13) { return 4; }
    if (BS != 92) { return 5; }
    if (SQ != 39) { return 6; }

    var x: u64 = 7;
    var y: f64 = (f64)x;
    if (y != y) { return 7; }

    return 0;
}

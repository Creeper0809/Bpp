// Covers: method overloading by arity in impl methods
// Expect exit code: 0

struct Acc {
    public base: i64;
}

impl Acc {
    public func sum(self: *Acc, a: i64) -> i64 {
        return self.base + a;
    }

    public func sum(self: *Acc, a: i64, b: i64) -> i64 {
        return self.base + a + b;
    }
}

func main() -> i64 {
    var acc: Acc;
    acc.base = 10;
    if (acc.sum(2) != 12) { return 1; }
    if (acc.sum(2, 3) != 15) { return 2; }
    return 0;
}
